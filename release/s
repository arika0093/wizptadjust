riot.tag2('nextgacha', '<loading if="{isLoading}"></loading><header class="dark"></header><main><div><span class="title">characters </span><checkutil mode="button" changed="{() =&gt; this.update()}" val="isBandSort">バンド順並び替え</checkutil></div><section class="charas"><div class="chara" each="{o, i in charaIdsList}" riot-style="--lrsd: {o.lrsDiffToday}; --order: {!isBandSort ? i : o.chbst}"><img riot-src="{iconUrl.replace(\'{0}\', o.cid)}"><br><span>{o.lrsFormat} <span class="small"> ({o.lrsDiffToday}日前)</span></span></div></section><span class="title">箱イベ</span><section class="bands"><div class="band" each="{o in bandsList}"><img riot-src="/static/thumb_band_{o.bid + 1}.png"><br><span>{o.lrsFormat} <span class="small"> ({o.lrsDiffToday}日前)</span></span></div></section><span class="title">属性</span><section class="attrs"><div class="attr" each="{a in attrsList}"><img riot-src="/static/thumb_{a.attr}.png"><br><span>{a.lrsFormat} <span class="small"> ({a.lrsDiffToday}日前)</span></span></div></section></main><footer></footer>', '* nextgacha,* [data-is="nextgacha"]{ display: flex; flex-direction: column; background-color: #000; color: #EEE; align-items: center; } nextgacha main,[data-is="nextgacha"] main{ max-width: 1200px; text-align: center; display: flex; flex-wrap: wrap; justify-content: center; flex-direction: column; padding-top: 1ex; } nextgacha section,[data-is="nextgacha"] section{ display: flex; flex-wrap: wrap; justify-content: center; flex-direction: row; margin-bottom: 5ex; } nextgacha .chara,[data-is="nextgacha"] .chara{ //display: inline-flex; flex-wrap: wrap; justify-content: space-between; margin: .5ex; padding: .5ex; align-items: center; width: 200px; border: 1px solid rgb(calc(var(--lrsd) * 1.5), 64, calc(255 - var(--lrsd) * 1.5), 1); background-color: rgb(calc(var(--lrsd) * 1.5), 64, calc(255 - var(--lrsd) * 1.5), 0.3); border-radius: 4px; order: var(--order); } nextgacha .small,[data-is="nextgacha"] .small{ font-size: smaller; } nextgacha checkutil,[data