    var $ = require("jquery");
    var calcTotal = require("../../js/scorecalc/bndrTotalValueCalc.js");
    var obs = global.Observer;
    this.setCards = [];

    this.on("mount", () => {

    })

    obs.on("cardDataSetFromCardView", (c, skillLv, ref) => {
    	var refs = ["card01", "card02", "card03", "card04", "card05"];
    	var i = refs.indexOf(ref);
    	c.skillLevel = skillLv;
    	this.setCards[i] = c;

