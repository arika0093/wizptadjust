EE; } chartview main,[data-is="chartview"] main{ display: flex; flex-direction: row; padding: 1ex; max-width: 1000px; margin: 0 auto; flex-grow: 1; } chartview viewspectram,[data-is="chartview"] viewspectram{ width: 400px; height: 600px; max-width: 100%; margin: 0; padding: 0; } chartview songdetail,[data-is="chartview"] songdetail{ display: flex; flex-direction: row; flex-grow: 1; padding: 0 1ex; } chartview img.jacket,[data-is="chartview"] img.jacket{ width: 350px; } chartview img.jacketmin,[data-is="chartview"] img.jacketmin{ width: 40px; } chartview titledatas p,[data-is="chartview"] titledatas p{ margin: 0; padding: 0; } chartview p.id,[data-is="chartview"] p.id,chartview p.tag,[data-is="chartview"] p.tag{ color: #BBB; font-size: 0.75rem; margin-top: .2ex; } chartview p.id::before,[data-is="chartview"] p.id::before{ content: "ID:"; } chartview p.title,[data-is="chartview"] p.title{ font-weight: bolder; display: inline-block; margin-bottom: .5ex; } chartview p.difficulty,[data-is="chartview"] p.difficulty,chartview p.level,[data-is="chartview"] p.level{ display: inline-block; border-radius: 4px; margin: 1ex 0 1ex 1ex; border: 1px solid #FFF; padding: .2ex .5ex; font-size: 0.9rem; text-transform: uppercase; } chartview p.notes,[data-is="chartview"] p.notes{ font-size: 0.85rem; } @media screen and (max-width: 800px) { chartview main,[data-is="chartview"] main{ flex-direction: column; margin: 0; } chartview viewspectram,[data-is="chartview"] viewspectram,chartview songdetail,[data-is="chartview"] songdetail{ width: 100%; height: calc(100vh - 90px); margin-bottom: 50px; } chartview p.mltitle,[data-is="chartview"] p.mltitle{ width: calc(100% - 200px - 40px); font-size: small; } } chartview music,[data-is="chartview"] music{ display: flex; flex-direction: row; align-items: center; border-bottom: 1px solid #888; } chartview music:last-child,[data-is="chartview"] music:last-child{ border-bottom: 0px; } chartview .isCover,[data-is="chartview"] .isCover{ background-color: