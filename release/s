 } header.dark a.t { color: #66a3ff; font-size: 1rem; } header a.t span.desc,[data-is="header"] a.t span.desc{ font-size: 0.7rem; margin: 0 5px; vertical-align: middle; } header a.t span.desc::before,[data-is="header"] a.t span.desc::before{ content: "≫ "; font-size: 0.7rem; } header nav,[data-is="header"] nav{ flex-grow: 1; text-align: right; padding: 0 1ex; font-size: 0.85rem; } header nav a.active,[data-is="header"] nav a.active{ border-radius: 4px; background: #0052cc; color: white; } header .link .dropdown-menu,[data-is="header"] .link .dropdown-menu{ display: none; background: #F0F0F0; position: absolute; right: 0; padding: 3px 5px; width: 100%; } header.dark .link .dropdown-menu { background: #1F1F1F; } header .link:hover .dropdown-menu,[data-is="header"] .link:hover .dropdown-menu,header .link:focus .dropdown-menu,[data-is="header"] .link:focus .dropdown-menu{ display: block; } header.withoutSubmenu .subexist { display: none; } @media screen and (max-width: 600px) { header nav,[data-is="header"] nav{ display: none; } }', '', function(opts) {
