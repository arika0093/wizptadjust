    var dateUtil = require("date-utils");
    var $ = require("jquery");
    var accessApi = require("../../js/bndrUtil/getJsonFromMultiServer.js");
    var bacName = require("../../js/bndrUtil/bandAndCharaName.js");

    const BDB_ICON = `https://bandori.ga/statics/chara_icon_{0}.png`;
    const BDR_ICON = `${global.RESRROOT}/res/icon/chara_icon_{0}.png`;

    const ReleasedTargetSince = "1514700000000";
    const IgnoreReleasedDate = ["4128645600000"];

    const CharacterNum = 30;
    const BandsIdList = [0,1,3,4,2,5];

    this.iconUrl = BDR_ICON;
    this.isBandSort = false;

    document.title = "Next ☆4";

    (async () => {
    	var cards = this.cardsList = (await accessApi.getCards([4])).data.sort((a,b) => {
    		return (b.releasedAt - 0) - (a.releasedAt - 0);
    	});
    	var releasedArray = Array.from(new Set(cards.map(e => e.releasedAt)))
    	var r4Array = releasedArray
    		.filter(e => ((e-0) >= (ReleasedTargetSince-0)) && !IgnoreReleasedDate.includes(e))
    		.map(e => cards.filter(t => t.releasedAt == e)
    			.map(t => ({
    				cid: t.characterId,
    				bid: bacName.charaId2BandId(t.characterId),
    				attr: t.attr,
    				release: t.releasedAt,
    				isdr: (t.skill.skillId === 20) || (t.skill.skillId >= 27 && t.skill.skillId <= 36),
    				rarity: t.rarity,
    				card: t,
    			}))
    		);

    	var rs = R4S(r4Array);

    	this.charaIdsList = rs.charasLatestRelease;
    	this.bandsList = rs.boxEventBandIds.slice(0,5).reverse();
    	this.attrsList = rs.attrLists.slice(0, 5).reverse();
    	this.update();

    })();

    function R4S(rar, option = {}) {

    	const AttrList = ["powerful", "cool", "pure", "happy"];
    	var attrs = $.extend([], AttrList);
    	var charas = new Array(CharacterNum).fill(1).map((e,i) => i + 1);

    	var r4ar = rar.map(e => e.filter(f => f.rarity == 4));

    	var nearCharaCount = option.nearCharaCount || 0;
    	var nearCharas = [].concat(...r4ar.filter((e,i) => i < nearCharaCount).map(e => {
    		return e.map(f => f.cid);
    	}));
    	charas = charas.filter(e => !nearCharas.includes(e))

    	var latestAttr = r4ar.map(e => e.find(f => !f.isdr)).filter(e => e)[0].attr;
    	attrs = attrs.filter(e => e !== latestAttr);

    	var eventType = r4ar.filter(e => {
    		return !e.every(f => f.isdr);
    	}).map(e => {
    		return e.filter(f => !f.isdr).every(f => e && e[0] && e[0].bid == f.bid);
    	});

    	var nextTypeIsBox = undefined;
    	if((eventType[0] == eventType[1])&& (eventType[1] == eventType[2])){
    		nextTypeIsBox = !eventType[0];
    	}

    	charas.sort((a,b) => {
    		var ats = rar.map(e_ => (e_.find(f => f.cid == a) || {}).release).filter(e => e)[0];
    		var bts = rar.map(e_ => (e_.find(f => f.cid == b) || {}).release).filter(e => e)[0];
    		return ats - bts;
    	})

    	var charasLatestRelease = charas.map(e => {
    		var ts = rar.map(e_ => (e_.find(f => f.cid == e) || {}).release).filter(e => e)[0];
    		var lrs = new Date(ts - 0);
    		var BandLists = BandsIdList;
    		var chbst = BandLists.findIndex(n => n == Math.ceil(e / 5) - 1) * 5 + ((e - 1) % 5);
    		return {
    			cid: e,
    			ts, lrs, chbst,
    			lrsFormat: lrs.toFormat("YY/MM/DD"),
    			lrsDiffToday: Math.floor((new Date() - lrs)/86400000),
    		}
    	});

    	var boxEventBandIds = r4ar.filter(e => {
    		var wo_dr = e.filter(f => !f.isdr)
    		var isbox = wo_dr.every(f => wo_dr[0] && wo_dr[0].bid == f.bid);
    		return isbox && !e.every(f => f.isdr);
    	}).map(e => {
    		var b = e.filter(f => !f.isdr).find(f => f.bid >= 0);
    		var lrs = new Date(b.release - 0);
    		return {
    			bid: b.bid,
    			lrs,
    			lrsFormat: lrs.toFormat("YY/MM/DD"),
    			lrsDiffToday: Math.floor((new Date() - lrs) / 86400000),
    		};
    	}).sort((a,b) => {
    		return b.lrs - a.lrs;
    	});

    	var attrLists = r4ar.map(e => {
    		var wo_dr = e.filter(f => !f.isdr);
    		var c = wo_dr.find(e => 1);
    		if(!c) { return null; }
    		var lrs = new Date(c.release - 0);
    		return {
    			attr: c.attr,
    			lrs,
    			lrsFormat: lrs.toFormat("YY/MM/DD"),
    			lrsDiffToday: Math.floor((new Date() - lrs) / 86400000),
    		};
    	}).filter(e => e).sort((a,b) => {
    		return b.lrs - a.lrs;
    	});

    	return {
    		charas, attrs, nextTypeIsBox, charasLatestRelease,
    		boxEventBandIds, attrLists
    	}
    }
});
}).call(this,typeof global !== "undefined" ? global : typeof self !== "undefined" ? self : typeof window !== "undefined" ? window : {})
