riot.tag2('checkutil', '<input class="{opts.mode}" id="{opts.tid || genMD5id()}" type="checkbox" checked="{opts.checked || false}" onchange="{onChanged}"><label class="{opts.mode}" for="{opts.tid || genMD5id()}"><yield></yield></label>', 'checkutil label,[data-is="checkutil"] label,checkutil input,[data-is="checkutil"] input{ cursor: pointer; display: inline-block; vertical-align: middle; } checkutil label.button,[data-is="checkutil"] label.button{ padding: .5ex 1ex; border: 1px solid #FFF; border-radius: 5px; margin-right: .75ex; } checkutil input.button,[data-is="checkutil"] input.button{ display: none; } checkutil input.button:checked + label.button,[data-is="checkutil"] input.button:checked + label.button{ background-color: #06F; }', '', function(opts) {
    var $ = require("jquery");
    var crypto = require('crypto');

    const InitChecked = ((opts.checked || "false").toLowerCase() !== "false") || false;
    this.isChecked = InitChecked;

    this.on("mount", () => {
    	var s = (this.opts.val || []).split(".");

    	variableUpdate(s, InitChecked, this.parent);
    })

    this.onChanged = (e) => {
    	var t = $(e.target);
    	var s = (this.opts.val || []).split(".");
    	var c = t.prop("checked");
    	c = this.opts.rev ? !c : c;

    	variableUpdate(s, c, this.parent)

    	if(this.opts.changed){
    		this.opts.changed(e, c, s);
    	}

    	this.isChecked = !c;

    };

    this.genMD5id = (() => {

    	return genMD5(opts.val);
    }).bind(this);

    function variableUpdate(s, c, parent){
    	return s.reduce((a,x,i,arr) => {
    		if(arr.length === i + 1){
    			return a[x] = c;
    		} else {
    			return a[x];
    		}
    	}, parent);
    }

    function genMD5(str) {
    	const md5 = crypto.createHash('md5')
    	return md5.update(str, 'binary').digest('hex');
    }
});
