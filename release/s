var jc = require("js-combinatorics");
var numeral = require("numeral");
var clone = require("fast-clone");
var arraySum = require("../util/calcArraySum.js");
var bacName = require("../bndrUtil/bandAndCharaName.js");

var DefaultBestPatternNum = 10;
var DefaultPf2BadFrame = 7.5; // PF2.5+GR3+GD1+BD1
var DefaultGoodFrames = 1;
var DefaultBadFrames = 1;

module.exports = {
	// init on webworker
	scoreCalculateOnWebWorker: function (obj) {
		/*
		return new Promise((res, rej) => {
			let worker = new webworker(function () {
				this.onmessage = (opts) => {
					var {t, obj} = opts;
					t.scoreCalculate(obj).then(rst => {
