alog(m)}" ontouch="{() =&gt; showSongDialog(m)}"><span class="level">{m.level}</span><img class="clearstate" riot-src="static/clear_{m.clearstate || 0}m.png"><img class="jacket" riot-src="{m.songJacket}"><span class="title">{m.songTitleWithDiff}</span><div class="counts"><span class="great">{m.great >= 0 ? m.great : ⁗---⁗}</span><span class="good"> {m.good >= 0 ? m.good : ⁗---⁗}</span><span class="bad"> {m.bad >= 0 ? m.bad : ⁗---⁗}</span><span class="miss"> {m.miss >= 0 ? m.miss : ⁗---⁗}</span></div></song></section></main><dialog id="updatedialog"><div class="modal-header"><span class="modal-title">{selectedChart.songTitleWithDiff}</span><span class="close" onclick="{() =&gt; updateDialog.close()}">×</span></div><div class="modal-body"><div class="jackets"><img riot-src="{selectedChart.songJacket}"><a target="_blank" href="{selectedChart.chartUrl}">ChartView</a></div><div class="scores"><div class="items"><label for="score_great">Great:</label><input id="score_great" riot-value="{selectedChart.great || 0}" placeholder="Great" type="{\'number\'}"></div><div class="items"><label for="score_good">Good:</label><input id="score_good" riot-value="{selectedChart.good || 0}" placeholder="Good" type="{\'number\'}"></div><div class="items"><label for="score_bad">Bad:</label><input id="score_bad" riot-value="{selectedChart.bad || 0}" placeholder="Bad" type="{\'number\'}"></div><div class="items"><label for="score_miss">Miss:</label><input id="score_miss" riot-value="{selectedChart.miss || 0}" placeholder="Miss" type="{\'number\'}"></div><div class="items"><input id="is_fced" type="checkbox"><label for="is_fced">Full Combo</label></div><div class="items"><input id="is_cleared" type="checkbox"><label for="is_cleared">Cleared</label></div></div></div><div class="modal-footer"><button class="important" onclick="{updateScore}">Save</button><button onclick="{() =&gt; updateDialog.close()}">Cancel</button></div></dialog><footer></footer>', 'clearmanage main,[data-is="clearmanage"] m