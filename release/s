    var $ = require("jquery")
    var bacName = require("../../js/bndrUtil/bandAndCharaName.js");

    const TargetBandIds = [0, 1, 3, 4, 2, 5];
    const TargetBandShortNames = ["ppp", "afg", "psp", "ros", "hhw", "mrf"];

    this.minImageCount = 3;

    this.on("update", () => {
    	var cards = opts.cards && opts.cards.data || [];
    	this.types = ["powerful", "cool", "pure", "happy"];
    	this.bands = TargetBandShortNames;
    	this.imageSize = opts.imagesize;
    	this.isAfter = opts.isafter;
    	this.chkMode = opts.chkmode;
    	this.readOnly = opts.ronly;
    	this.isnfExist = cards.some(e => e.isnf);
    	updateCardslist.bind(this)(cards);

    });

    function updateCardslist(cards) {
    	if(cards.length <= 0){
    		return;
    	}

    	cards = cards.filter(e => !e.title.includes("ガルパ杯"));

    	this.cardsOfBand = TargetBandIds.map(n => {
    		return cards.filter(c => {
    			var bidOfCard = bacName.charaId2BandId(c.characterId);
    			return bidOfCard === n;
    		});
    	});
    	this.cardsOfType = this.cardsOfBand.map(e => {
    		return this.types.map(t => {
    			return e.filter(c => c.attr.indexOf(t) >= 0);
    		});
    	});

    	this.cardsOfChara = this.cardsOfType.map(t => {
    		return t.map(b => {
    			return [0,1,2,3,4].map((d, n) => {
    				return b.filter(c => ((c.characterId - 1) % 5) === n)
    					.sort((a, b) => {
    						if(a.rarity != b.rarity){
    							return b.rarity - a.rarity;
    						}
    						return (b.udt || {}).skillRate - (a.udt || {}).skillRate;
    					});
    			})
    		})
    	})

    }
});
