},{}],6:[function(require,module,exports){
(function (global){
var SongsAPI = (global.APIROOT) + "/music";
var ChartAPI = (global.APIROOT) + "/music/chart/{0}/{1}"; // 0: songid, 1: difficult
var Filename = "charts.json"; 

var $ = require("jquery");
var storageFile = require("../util/storageFile.js");
var stringFormat = require("../util/stringFormat.js");
var accessApi = require("./getJsonFromMultiServer.js");

module.exports = async function (filter, forceReload) {
	if ( filter === void 0 ) filter = "";
	if ( forceReload === void 0 ) forceReload = true;

	var songs = await accessApi.getSongs(); 
	try {
		var dlCharts = await storageFile.load(Filename) || {};
	} catch(e) {
		console.error("storage error: ", e);
		dlCharts = {};
	}
	
	// 曲ID+譜面レベル
	var diffsList = ["easy", "expert", "hard", "normal", "special"];
	var songWithDiff = songs.data.map(function (e) {
		return diffsList.map(function (d) { return ({
			id: e.musicId,
			diff: d,
			level: e.difficulty[diffsList.indexOf(d)],
			path: ((e.musicId) + "-" + d),
			isSpecialExist: (e.difficulty.length === 5),
		}); });
	}).reduce(function (a,x) {
		return a.concat.apply(a, x); 
	}, []);
	
	// 不足分を読み込み
	var dlPromise = songWithDiff.filter(function (e) {
		var isExist = (e.diff !== "special" || e.isSpecialExist);
		return (isExist && (forceReload || !dlCharts[e.path]) && e.path.includes(filter))
	}).map(async function(e) {
		var rst = await accessApi.getChart(e.id, e.diff);
		if(rst.objects.length > 0) {
			dlCharts[e.path] = {
				id: e.id,
				diff: e.diff,
				level: e.level,
				song: songs.data.find(function (s) { return s.musicId === e.id; }),
				chart: rst
			};
		} 
		return true;
	});
	// 終了待機
	await Promise.all(dlPromise);
	// save
	try {
		storageFile.save(Filename, dlCharts);
	} catch(e) {
		console.error("storage error: ", e);
	}

	return dlCharts;
};
}).call(this,typeof global !== "undefined" ? global : typeof self !== "undefined" ? self : typeof window !== "undefined" ? window : {})
},{"../util/storageFile.js":16,"../util/stringFormat.js":18,"./getJsonFromMultiServer.js":8,"jquery":171}],7:[function(require,module,exports){
(function (global){
var numeral = require("numeral");
var stringFormat = require("../util/stringFormat.js");

module.exports = function (card, isAfterTraining) {
	if ( isAfterTraining === void 0 ) isAfterTraining = true;

	var par = card.imageUrlParentDir || ((global.RESROOT) + "/assets-jp");
	var CardImageURL = par + "/thumb/chara/card{0}_rip/" +
		"{1}_" + (isAfterTraining && card.rarity >= 3 ? "after_training" : "normal") + ".png";
	return stringFormat(CardImageURL, numeral(calcCardGroup(card.cardId)).format("00000"), card.cardRes);
};

// calc cardGroup
function calcCardGroup(cardId){
	return Math.trunc(cardId / 50).toString();
}
