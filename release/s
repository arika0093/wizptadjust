nsmore.showed{ display: block; } viewspectram vsoptionsmore > .option,[data-is="viewspectram"] vsoptionsmore > .option{ display: flex; flex-direction: row; align-items: center; flex-wrap: nowrap; justify-content: space-between; padding: 1ex 0; border-bottom: 1px solid #888; flex-shrink: 1; background: rgba(0, 0, 0, 0.5); } viewspectram vsoptionsmore > .option:last-child,[data-is="viewspectram"] vsoptionsmore > .option:last-child{ border-bottom: 0px none transparent; } viewspectram vsoptionsmore > .option > .information,[data-is="viewspectram"] vsoptionsmore > .option > .information{ display: flex; flex-direction: column; align-items: flex-start; padding-right: .5ex; } viewspectram vsoptionsmore > .option > .information > p.i_main,[data-is="viewspectram"] vsoptionsmore > .option > .information > p.i_main{ margin: 0 .5ex 0 0; font-weight: bolder; font-size: .95rem; color: #6CF; } viewspectram vsoptionsmore > .option > .information > p.i_sub,[data-is="viewspectram"] vsoptionsmore > .option > .information > p.i_sub{ margin: 0; font-size: .72rem; } viewspectram vsoptionsmore > .option > .forms,[data-is="viewspectram"] vsoptionsmore > .option > .forms{ flex-shrink: 0; } viewspectram vsoptionsmore > .option > .forms > input[type="number"],[data-is="viewspectram"] vsoptionsmore > .option > .forms > input[type="number"]{ width: 50px; margin-right: 1ex; } viewspectram checkutil label.button,[data-is="viewspectram"] checkutil label.button{ font-size: .85rem; background-color: #000; }', '', function(opts) {
    var $ = require("jquery");
    var numeral = require("numeral");
    var {Howl, Howler} = require("howler");
    var stringFormat = require("../../js/util/stringFormat.js");

    this.BetweenNotes = 30;
    this.NotesWidth = 30;
    this.NotesHeight = 10;
    this.LaneCoverPerc = 0;
    this.TopMargin = 1000;
    this.BottomMargin = 125;
    this.BMFixed = +2+5;
    this.settingShowed = false;
    this.normalShowed = true;
    this.overlayShowed = !!(opts.overlay);
    this.skillAreaExisted = this.overlayShowed || (opts.score && !!opts.score.objects.find(e => e.scoreUpStart));

    this.laneCoverOptions = [
    	{name: "black", desc: "黒単色"},
    	{name: "grad",  desc: "グラデーション"},

    ];
    this.justLineOptions = [
    	{name: "none", desc: "透明"},
    	{name: "black", desc: "黒単色"},
    	{name: "grad", desc: "グラデーション"},
    ];

    this.sameLineShowed = false;
    this.isPlayBGM = true;
    this.isPlayNotesSound = true;
    this.isPlayLNotesSound = true;
    this.isMirror = false;
    this.isAutoRepeat = false;
    this.playSound = null;
    this.isPlayingNow = false;
    this.selectedLaneCoverOpt = "black";
    this.selectedJustLineOpt = "none";

    var objs = $.extend([], opts.chart.objects);
    this.songTimeFPS = Math.ceil((objs.reverse()[0].timing + 1) * 60);

    this.on("mount", () => {
    	setTimeout(() => this.moveToPlayTime(0), 1);
    });
    this.on("before-unmount", () => {
    	this.stopButtonPush();
    });
    this.on("update", () => {

    })

    var sh = 0;
    this.spectramHeightCalc = (refresh) => {
    	if(refresh || sh <= 0){
    		var ob = $.extend(true, [], opts.chart.objects);
    		var l_t = ob.sort((a,b) => b.timing - a.timing)[0].timing;
    		sh = this.calclateNotePosition(l_t) + this.TopMargin + this.BottomMargin;
    	}
    	return sh;

    };

    this.genNoteClass = (note, i) => {
    	var e = note.effect || "", p = note.property || "";
    	var isLN_SE = p.match(/Long(Start|End)/) || e.match(/Slide(Start|End)/);
    	var isSystem = note.type === "System";
    	var isSkill  = e.indexOf("Skill") >= 0;
    	var isFlick  = e.indexOf("Flick") >= 0;
    	var isSlide  = e.match(/Slide_[AB]/);
    	var isFlickSlideEnd = isFlick && e.match(/SlideEndFlick_/);
    	var isContainSkill = !!note.scoreUpRate;
    	var isJudgeGreat = (note.judge || "").includes("great");
    	var isActive = (note.index >= 0 && note.index === this.activeNote);
    	var styles = [
    		isSystem ? "System" : null,
    		isLN_SE ? "Long" : null,
    		isSkill ? "Skill" : null,
    		isFlick ? "Flick" : null,
    		isSlide ? "Slide" : null,
    		isFlickSlideEnd ? "FrickEnd" : null,
    		isJudgeGreat ? "Great" : null,
    		isContainSkill ? "SkillContain" : null,
    		isActive ? "clickActive" : null,
    	];
    	return styles.filter(e => e).join(" ");
    };

    this.genNoteStylePosition = (n, t, i) => {
    	var fmt_str = n.diffFrame % 1 != 0 ? "+0.0" : "+0";
    	var df = (n.diffFrame || 0) !== 0 ? `"${numeral(n.diffFrame).format(fmt_str)}"` : " ";
    	var style = `
    		--bpos:  ${this.calclateNotePosition(t)}px;
    		--width: ${this.NotesWidth}px;
    		--height:${this.NotesHeight}px;
    		--lane:  ${n.lane};
    		--index: ${i};
    		--diff:  ${df};
    	`;
    	return style.replace(/(\s{2,}|\t|\n|\r)/g, "");
    };

    this.generateLongObject = (() => {
    	var o = opts.chart.objects;
    	var os = o.filter(e => {
    		return (
    			(e.property == "Slide" && !e.effect.includes("Start")) ||
    			(e.property.includes("LongEnd"))
    		);
    	});
    	return os.map(e => {

    		var i = o.findIndex(o => o.timing === e.timing && o.lane === e.lane);

    		var isLong = !e.property.includes("Slide");
    		var bn = o.filter((e_,i_) => i_ < i).reverse().find(ef => {
    			if(isLong) {

    				return ef.lane === e.lane && ef.property === "LongStart";
    			} else {
    				try {

    					const findAB = (s) => {
    						var m = s.match(/Slide(Start|End|)(Flick|)_(A|B)/);
    						return m ? m[3] : null;
    					};
    					const AB = findAB(e.effect);
    					var reg = new RegExp(`Slide(Start|)_${AB}`);
    					return ef.effect.match(reg);
    				} catch(e) {
    					console.error(e);
    				}
    			}
    		});

    		if(!e || !bn){
    			return { style: "", error: "e/bn is undefined" }
    		}
    		var w = this.NotesWidth,
    			h = this.NotesHeight;
    		var lw = w;
    		var ep = this.calclateNotePosition(e.timing),
    			bp = this.calclateNotePosition(bn.timing);
    		var dh = Math.abs(bp - ep);
    		var ld = e.lane - bn.lane;

    		var matrixJX = (-(ld * lw) / (dh));
    		var topDiff = -(ld * lw) / 2;
    		var style = `
    			--bottom: ${ep}px;
    			--lane: ${e.lane};
    			--width: ${lw}px;
    			--height: ${dh}px;
    			--noteheight: ${h}px;
    			transform: matrix(1, 0, ${matrixJX}, 1, ${topDiff}, 0);
    		`;
    		style = style.replace(/(\s{2,}|\t|\n|\r)/g, "");
    		return {
    			style,
    		}
    	});
    }).bind(this);

    this.generateSameObjLine = () => {
    	var o = opts.chart.objects;
    	var os = o.filter(e => {
    		return !e.effect.includes("Slide_");
    	}).map(e => {
    		var sameObj = o.filter(e => {
    			return !e.effect.includes("Slide_");
    		}).find(es => es.timing === e.timing);
    		if(!sameObj){
    			return null;
    		}
    		var et = this.calclateNotePosition(e.timing);
    		var l1 = e.lane, l2 = sameObj.lane;
    		var w = this.NotesWidth;
    		var h = this.NotesHeight;
    		var style = `
    			--bottom: ${et}px;
    			--lane1: ${Math.min(l1, l2)};
    			--lane2: ${Math.max(l1, l2)};
    			--width: ${w}px;
    			--noteheight: ${h}px;
    		`;
    		style = style.replace(/(\s{2,}|\t|\n|\r)/g, "");
    		return {style};
    	});
    	return os.filter(e => e);
    }

    this.generateBeatLines = (() => {
    	var rst = [];
    	var o = opts.chart.objects;
    	var bpm = opts.chart.metadata.bpm;
    	var bpmc = bpm;
    	var timingOnBeatFirst = 0;
    	var lastNote = o.filter(e => e.type !== "System").reverse()[0];
    	var lastBeat = Math.floor(lastNote.beat);
    	var lastBeatNext = lastBeat + 30
    	for(var b = 0; b <= lastBeatNext; b++){

    		var bpmChange = o.find(e => {
    			return e.effect === "BPMChange" && e.beat >= b && e.beat < b+1;
    		});
    		if(bpmChange){
    			bpm = bpmc = bpmChange.value;
    		}

    		var secondsOfThisBeat = 60 / bpm;

    		rst = rst.concat(...[0, 0.5].map(bt => {
    			var isHalf = bt === 0.5;
    			var isFirst = (b % 4 === 0 && !isHalf);
    			var cls = isHalf ? "half" : isFirst ? "onf" : "";
    			var timing = timingOnBeatFirst + secondsOfThisBeat * bt;
    			var pos = this.calclateNotePosition(timing);
    			var w = this.NotesWidth, h = this.NotesHeight;
    			var style = `
    				--bottom: ${pos}px;
    				--width: ${w}px;
    				--noteheight: ${h}px;
    				${isFirst ? `--pacen: "${b/4}";` : ""}
    				${!isHalf && bpmc > 0 ? `--bpmc: "BPM:${bpmc}";` : ""}
    			`;
    			style = style.replace(/(\s{2,}|\t|\n|\r)/g, "");
    			return {
    				"class": cls,
    				style,
    			}
    		}));
    		timingOnBeatFirst += secondsOfThisBeat;
    		bpmc = null;
    	}
    	return rst;
    }).bind(this);

    this.genSkillAreaObj = () => {
    	if(!(this.overlayShowed && opts.overlay) && !opts.score){
    		return [];
    	}
    	var sc = (this.overlayShowed) ? opts.overlay : opts.score.objects;
    	var areas = sc.filter(e => {
    		return e.scoreUpStart && e.scoreUpEnd;
    	}).map(e => {
    		return {
    			s: e.scoreUpStart,
    			e: e.scoreUpEnd,
    			s_p: this.calclateNotePosition(e.scoreUpStart),
    			s_e: this.calclateNotePosition(e.scoreUpEnd),
    		};
    	});
    	var cleanList = areas.filter(function(v1, i1, a1){
    		return (a1.findIndex(function(v2){
    			return (v1.s === v2.s)
    		}) === i1);
    	});
    	cleanList.forEach((e,i) => {
    		e.index = i + 1;
    	})
    	return cleanList;
    };

    this.moveToAnchor = (i, a) => {
    	var t = $(`#S${i}${a}`);
    	var ps = this.playSound;
    	var v = t.attr("time") - 1.5;

    	$("#songTimes").val(Math.floor(v * 60));
    	ps && ps.seek(v);

    	t[0].scrollIntoView(true);
    }

    this.onNoteClicked = (note) => {
    	return (event) => {
    		var score = note.calcScore;
    		var rate = (note.skillRate/100 || 0) + 1;
    		var base = Math.round(score/rate);
    		var judge_short = (note.judge.includes("great")) ? "GR" : "PF";

    		this.noteDetailInfo = {
    			isSkillContained: rate > 1,
    			combo: note.cmb,
    			cmbrate: note.comboRate,
    			score,
    			basescore: base,
    			judge_short,
    			uprate: numeral(rate).format("0.00"),
    			diff_f: numeral(note.diffFrame).format("+0.0"),
    		};
    		this.activeNote = note.index;
    		this.update();
    	}
    };
    this.closeNoteDetailInfo = () => {
    	this.noteDetailInfo = null;
    	this.activeNote = undefined;
    	this.update();
    };

    var vs = null, vh = 0;
    this.moveToPlayTime = (t) => {
    	var h = this.calclateNotePosition(t);
    	var sh = this.spectramHeightCalc();
    	vs = vs || $("vsMain");
    	vh = vh || vs.height();
    	var v = (sh - h - vh + this.BottomMargin);
    	requestAnimationFrame(() => {
    		vs[0].scrollTop = v;
    	});
    };

    this.seekChanged = (e) => {

    	var ps = this.playSound;
    	var st = $("#songTimes")
    	if(ps && ps.playing() && e.clientX) {
    		var w = st.width();
    		var c = e.offsetX;
    		var d = ps.duration();
    		ps.seek(d * c/w);
    	} else {
    		var v = st.val() - 0;
    		this.moveToPlayTime(v / 60);
    		ps && ps.seek(v/60);
    	}
    };

    this.calclateNotePosition = (t) => {
    	return (t * this.BetweenNotes * this.NotesHeight) + this.BottomMargin;
    };

    this.playButtonPush = async () => {
    	var t = $("#songTimes").val() - 0;
    	if(!this.playSound){

    		var bgm = "";
    		if(opts.id){
    			bgm = `bgm${numeral(opts.id).format("000")}`
    		} else {
    			bgm = opts.chart.metadata.bgm.replace(".wav", "");
    		}

    		var resourceURL = `sound/${bgm}_rip/${bgm}.mp3`;
    		var soundURLs = [
    			`${global.RESROOT}/assets/${resourceURL}`,
    			`${global.IGNORECORS}${global.RESRROOT}/assets/jp/${resourceURL}`
    		];

    		this.playSound = new Howl({
    			src: soundURLs,
    			loop: this.isAutoRepeat,
    			mute: !this.isPlayBGM,
    			onplay: () => {

    				this.isLoading = false;
    				this.isPlayingNow = true;

    				this.onPlayingStep();
    				this.onPlayingNotesActions(true);
    				this.update();

    				this.laneCoverSetting();
    			},
    			onseek: () => {
    			},
    			onend: () => {

    				if(!this.playSound.loop()){
    					this.isPlayingNow = false;
    					this.playSound = null;
    				}
    				this.update();
    			},
    		});

    		this.SERegist();
    		this.notesAction(-1);

    		var i = this.playSound.play();
    		this.playSound.seek(t/60, i);

    		this.isLoading = true;
    		this.update();
    	} else {

    		var ps = this.playSound;
    		if(ps.playing()){
    			ps.pause();
    			this.isPlayingNow = false;
    		} else {
    			ps.play();
    			this.isPlayingNow = true;
    		}
    		this.update();
    	}
    };

    this.stopButtonPush = () => {
    	var ps = this.playSound;
    	if (ps) {
    		ps.stop();
    		this.isPlayingNow = false;
    		this.moveToPlayTime(0);
    	}
    }

    var st = null, bseek = -1;
    this.onPlayingStep = () => {
    	var ps = this.playSound;
    	var seek = ps ? ps.seek() || 0 : 0;

    	this.moveToPlayTime(seek);

    	st = document.getElementById("songTimes");
    	if(st){
    		st.value = Math.floor(seek * 60);
    	}

    	if(ps && ps.playing()){
    		requestAnimationFrame(this.onPlayingStep);
    	}
    };
    this.onPlayingNotesActions = (bskf) => {
    	var ps = this.playSound;
    	var seek = ps ? ps.seek() || 0 : 0;
    	bseek = bskf ? seek : bseek;

    	this.notesAction(seek, bseek);
    	if (ps && ps.playing()) {
    		setTimeout(this.onPlayingNotesActions, 5);
    	}
    	bseek = seek;
    }

    var SE = {
    	normal: {
    		name: "hihat.wav", volume: 60,
    	},
    	flick: {
    		name: "flick.mp3", volume: 80,
    	},
    	skill: {
    		name: "hihat.wav", volume: 100,
    	},
    };
    this.SERegist = () => {
    	const CORSP = `https://cors-anywhere.herokuapp.com/`;
    	const urlBase = `https://bandori.ga/statics/`;
    	if (!SE.normal.sound) {

    		Object.keys(SE).forEach(key => {
    			var se = SE[key];
    			var url_b = `${CORSP}${urlBase}${se.name}`;
    			var url = stringFormat(url_b, 1);
    			se.sound = new Howl({
    				src: [url],
    				volume: (se.volume || 100) / 100,
    			});
    		})
    	}
    },

    this.notesAction = (t, b) => {
    	if(!this.isPlayNotesSound) return;

    	var objs = (opts.overlay || opts.chart.objects);

    	var key = "timing";
    	var tmap = new Set(objs.map(e => e[key]));
    	var oflt = objs.filter(e => {
    		return !(e.type && e.type.includes("System")) && e.timing > b && e.timing <= t;
    	}).filter(e => {
    		var ln = this.isPlayLNotesSound;
    		if(ln){ return true; }
    		var isSlideTickOrEnd = e.effect.match(/(Slide_[AB]|SlideEnd)/);
    		var isLongEnd = e.property.match(/LongEnd/);
    		return !(isSlideTickOrEnd || isLongEnd);
    	}).sort((a,b) => {

    		if(a.effect.includes("Flick")) return -1;
    		if(b.effect.includes("Flick")) return +1;
    	});
    	if(oflt.length <= 0) return;
    	var e = oflt[0];
    	var key = e.effect.includes("Flick") ? "flick" :
    			  e.effect.includes("Skill") ? "skill" : "normal";
    	(async () => {

    		SE[key].sound.play();
    	})();
    };

    this.onKeyPressAction = (e) => {
    	var l = this.LaneCoverPerc, p = this.playSound;
    	if(!p) return;
    	switch(e.keyCode){
    		case 37:

    			this.LaneCoverPerc = Math.max(l-5, 0);
    			break;
    		case 39:

    			this.LaneCoverPerc = Math.min(l+5, 100);
    			break;
    		default:

    			return false;
    	}
    	this.laneCoverSetting();
    	e.preventUpdate = true;
    	return true;
    };

    this.laneCoverSetting = () => {

    	var lc = $("vsLaneCover");
    	if (lc) {
    		lc.css("height", `${this.LaneCoverPerc}%`);
    	}
    };

    this.toggleSettingView = () => {
    	var s = this.settingShowed = !this.settingShowed;

    	if(!s){

    		this.BetweenNotes = $("#betweenNotes").val() - 0;
    		this.LaneCoverPerc = $("#laneCoverPerc").val() - 0;
    		this.laneCoverSetting();
    		this.spectramHeightCalc(true);
    	}
    	this.update();
    };

    this.optionChanged = () => {
    	var ps = this.playSound;
    	if(ps){
    		ps.loop(this.isAutoRepeat);
    		ps.mute(!this.isPlayBGM);
    	}

    	this.update();
    };

    this.mirrorToggle = (() => {
    	var mirrorObjects = (o) => {
    		o && o.forEach(e => {
    			e.lane = e.lane >= 1 ? 8 - e.lane : undefined;
    		});
    	};
    	mirrorObjects(opts.chart && opts.chart.objects);
    	mirrorObjects(opts.overlay);
    	mirrorObjects(opts.score && opts.score.objects);

    	this.update();
    }).bind(this);
});
}).call(this,typeof global !== "undefined" ? global : typeof self !== "undefined" ? self : typeof window !== "undefined" ? window : {})
