e"] song.sp:hover{ background: rgba(255, 0, 255, 0.15); } clearmanage song:last-child,[data-is="clearmanage"] song:last-child{ border-bottom: 1px none transparent; } clearmanage song:nth-child(even),[data-is="clearmanage"] song:nth-child(even){ background: #111; } clearmanage img.clearstate,[data-is="clearmanage"] img.clearstate{ width: 17px; margin: 0 .2ex; } clearmanage img.jacket,[data-is="clearmanage"] img.jacket{ width: 25px; height: 25px; margin: 0 .3ex; display: none; } clearmanage img.icon,[data-is="clearmanage"] img.icon{ width: 12px; } clearmanage span.level,[data-is="clearmanage"] span.level{ display: inline-block; border-radius: 4px; border: 1px solid #FFF; padding: .2ex; margin: 0 .3ex; font-size: 0.9rem; text-transform: uppercase; } clearmanage .ex > span.level,[data-is="clearmanage"] .ex > span.level{ color: #FFF; } clearmanage .sp > span.level,[data-is="clearmanage"] .sp > span.level{ border-color: #F0F; color: #FFF; } clearmanage span.title,[data-is="clearmanage"] span.title{ flex-grow: 1; margin: 0 .3ex; font-size: 0.9rem; } clearmanage div.counts,[data-is="clearmanage"] div.counts{ margin: 0 .5ex; min-width: 150px; } clearmanage .counts > span,[data-is="clearmanage"] .counts > span{ width: 35px; display: inline-block; text-align: right; font-family: "Iceland"; font-size: 1.2rem; } clearmanage song.header,[data-is="clearmanage"] song.header{ cursor: default; border-bottom: 1px solid #AAA; } clearmanage song.header .title,[data-is="clearmanage"] song.header .title{ text-transform: uppercase; font-family: "Iceland"; margin-left: 90px; font-size: 1rem; } clearmanage song.header div.counts span,[data-is="clearmanage"] song.header div.counts span{ font-size: 0.8rem; text-transform: uppercase; font-family: "Iceland"; } clearmanage dialog,[data-is="clearmanage"] dialog{ padding: 0; border: 0; border-radius: 0.6rem; box-shadow: 0 0 1em black; max-width: 400px; width: 100%; } clearmanage dialog::backdrop,[data-is="clearmanage"] dialog::backdrop{ background-color: rgba(0, 0, 0, 0.4); } clearmanage dialog[open],[data-is="clearmanage"] dialog[open]{ animation: slide-up 0.4s ease-out; } clearmanage .modal-header,[data-is="clearmanage"] .modal-header{ padding: 1ex; } clearmanage .modal-body,[data-is="clearmanage"] .modal-body{ padding: 1ex; min-height: 200px; display: flex; flex-direction: row; justify-content: center; align-items: center; } clearmanage .modal-footer,[data-is="clearmanage"] .modal-footer{ padding: 1ex; display: flex; flex-direction: row; justify-content: flex-end; align-items: center; } clearmanage span.modal-title,[data-is="clearmanage"] span.modal-title{ font-size: 1.2rem; padding: .5ex; font-weight: bolder; color: #048; } clearmanage .close,[data-is="clearmanage"] .close{ position: absolute; top: 0.2em; right: 0.2em; padding: 0.3em; line-height: 0.6; background-color: transparent; border: 0; font-size: 1.2em; cursor: pointer; } clearmanage .modal-header,[data-is="clearmanage"] .modal-header{ margin: 0; padding-bottom: 0.6em; background-color: #CEF; border-top-left-radius: 0.6rem; border-top-right-radius: 0.6rem; border-bottom: 1px solid #8CF; } clearmanage .modal-footer,[data-is="clearmanage"] .modal-footer{ border-top: 1px solid #8CF; } clearmanage dialog button,[data-is="clearmanage"] dialog button{ margin: .5ex; padding: .5ex 1ex; background-color: transparent; border: 1px solid #888; border-radius: 4px; cursor: pointer; } clearmanage dialog button:hover,[data-is="clearmanage"] dialog button:hover{ filter: brightness(110%); } clearmanage dialog button.important,[data-is="clearmanage"] dialog button.important{ border-color: #08F; background-color: #DEF; } clearmanage dialog button.danger,[data-is="clearmanage"] dialog button.danger{ border-color: #F00; background-color: #FDC; } clearmanage div.jackets,[data-is="clearmanage"] div.jackets{ display: flex; flex-direction: column; justify-content: center; align-items: center; } clearmanage div.jackets img,[data-is="clearmanage"] div.jackets img{ width: 200px; } clearmanage div.jackets a,[data-is="clearmanage"] div.jackets a{ color: #08F; margin-top: 1ex; font-family: "Iceland"; font-size: 1.1rem; } clearmanage div.scores,[data-is="clearmanage"] div.scores{ display: flex; flex-direction: column; justify-content: center; width: 150px; } clearmanage div.scores .items,[data-is="clearmanage"] div.scores .items{ display: flex; flex-direction: row; justify-content: flex-end; align-items: center; margin: .5ex 0; } clearmanage div.scores label,[data-is="clearmanage"] div.scores label{ font-size: 1.2rem; font-family: "Iceland"; margin-right: 3px; } clearmanage div.scores input[type="number"],[data-is="clearmanage"] div.scores input[type="number"]{ text-align: right; width: 60px; } @keyframes slide-up { 0% { opacity: 0; transform: translate(0, 15px); } 100% { opacity: 1; transform: translate(0, 0); } }', '', function(opts) {
    var $ = require("jquery");
    var storage = require("../../js/util/storageJson.js");
    var sformat = require("../../js/util/stringFormat.js");
    var accessApi = require("../../js/bndrUtil/getJsonFromMultiServer.js");
    var findBandOrder = require("../../js/bndrUtil/bandOrder.js");

    this.options = {};
    this.counts = {};
    this.selectedChart = {};
    this.loading = true;
    var songsList = {}

    document.title = "BangDream ClearManager";

    this.on("mount", async () => {
    	songsList = await accessApi.getSongs();
    	this.exspList = updateExSpList(songsList.data);
    	countUpdate(this);
    	this.loading = false;
    	this.update();
    });

    this.onChangeOption = (() => {
    	var sortopt = $(`input[name='sortopt']:checked`).val();
    	this.exspList = updateExSpList(songsList.data, sortopt).filter(e => {
    		var o = this.options;
    		if(e.clearstate == 3 && !o.is_ap) return false;
    		if(e.clearstate == 2 && !o.is_fc) return false;
    		if(e.clearstate == 1 && !o.is_cl) return false;
    		if(!e.clearstate && !o.is_nc) return false;
    		return true;
    	});
    	countUpdate(this);
    	this.update();
    }).bind(this);

    this.showSongDialog = ((m) => {
    	this.selectedChart = m;
    	var ud = this.updateDialog = $("#updatedialog")[0];
    	this.update();
    	ud.addEventListener("mousedown", (event) => {
    		if(event.target === ud) {
    			ud.close('cancelled');
    		}
    	})
    	ud.showModal();
    }).bind(this);

    this.updateScore = (() => {
    	var m = this.selectedChart;
    	var d = this.updateDialog;
    	var clearstate = 0;
    	var savename = `savescore.${m.musicId}-${m.difficulty}`;
    	var great = $("#score_great").val() - 0;
    	var good = $("#score_good").val() - 0;
    	var bad = $("#score_bad").val() - 0;
    	var miss = $("#score_miss").val() - 0;
    	var is_fc = $("#is_fced").prop("checked");
    	var is_cl = $("#is_cleared").prop("checked");
    	var ap_date = m.ap_date;
    	var fc_date = m.fc_date;
    	if(Math.max(great, good, bad, miss) == 0){
    		clearstate = 3;
    	}
    	else if (Math.max(good, bad, miss) == 0 || is_fc){
    		clearstate = 2;
    	}
    	else if ((bad + miss * 2 < 20) || is_cl) {
    		clearstate = 1;
    	}

    	if(clearstate == 3 && !ap_date){
    		ap_date = (new Date()).getTime();
    	}
    	if (clearstate >= 2 && !fc_date) {
    		fc_date = (new Date()).getTime();
    	}

    	storage.save(savename, {
    		great, good, bad, miss, clearstate, fc_date, ap_date
    	})

    	this.selectedChart = {};
    	this.updateDialog = undefined;
    	d.close();

    	this.onChangeOption();

    }).bind(this);

    function updateExSpList(songsList, sortType = "clear", sortdir = "desc"){
    	var s = songsList.map(e => {
    		var r = []
    		var [esl, exl, hdl, nrl, spl] = e.difficulty;

    		if(exl > 0) {
    			r.push(extractSongData(e, "ex", exl));
    		}
    		if(spl > 0) {
    			r.push(extractSongData(e, "sp", spl));
    		}
    		return r;
    	});
    	var ss = s.reduce((a,x) => a.concat(...x), []);

    	ss.sort((a,b) => {

    		if(a.level > b.level) return -1
    		if(a.level < b.level) return +1

    		if(a.isCover && !b.isCover) return +1
    		if(!a.isCover && b.isCover) return -1

    		if(a.bandId < b.bandId) return -1
    		if(a.bandId > b.bandId) return +1

    		if(a.released < b.released) return -1
    		if(a.released > b.released) return +1
    	})

    	dir = sortdir == "asc" ? +1 : -1;
    	switch(sortType){
    		case "level":
    			ss.sort((a,b) => {
    				if(a.level > b.level) return +dir
    				if(a.level < b.level) return -dir
    			})
    			break;
    		case "clear":
    			ss.sort((a,b) => {

    				if(a.clearstate == undefined) return -dir
    				if(b.clearstate == undefined) return +dir
    				if(a.clearstate > b.clearstate) return +dir
    				if(a.clearstate < b.clearstate) return -dir

    				var aug = calcUnderGreatCount(a), bug = calcUnderGreatCount(b);
    				if(aug > bug) return -dir
    				if(aug < bug) return +dir

    				if ((a.great || 0) > (b.great || 0)) return -dir
    				if ((a.great || 0) < (b.great || 0)) return +dir
    			})
    			break;
    		case "update":
    			ss.sort((a,b) => {

    				if(a.clearstate == undefined) return -dir
    				if(b.clearstate == undefined) return +dir
    				if(a.clearstate > b.clearstate) return +dir
    				if(a.clearstate < b.clearstate) return -dir

    				if(a.clearstate == 3){
    					if (a.ap_date > b.ap_date) return +dir
    					if (a.ap_date < b.ap_date) return -dir
    				}
    				if(a.clearstate == 2){
    					if (a.fc_date > b.fc_date) return +dir
    					if (a.fc_date < b.fc_date) return -dir
    				}
    			})
    			break;
    	}
    	return ss;
    }

    function extractSongData(song, diff, level){
    	var musicId = song.musicId
    	var savedScore = storage.load(`savescore.${musicId}-${diff}`)
    	var {great, good, bad, miss, clearstate, fc_date, ap_date} = savedScore || {};
    	return {
    		musicId,
    		songTitle: song.title,
    		songTitleWithDiff: `${song.title}${diff === "sp" ? " [SP]" : ""}`,
    		songJacket: `${song.imageUrlParentDir}${song.jacket}`,
    		bandId: findBandOrder(song.bandId),
    		bgmFile: song.bgmFile,
    		chartUrl: `/#!/chartview/id:${musicId}/diff:${diff === "sp" ? "special" : "expert"}`,
    		released: new Date(song.publishedAt - 0),
    		difficulty: diff,
    		level: level,
    		isCover: song.tag === "anime",
    		great, good, bad, miss,
    		clearstate,
    		fc_date, ap_date
    	}
    }

    function countUpdate(t){

    	var ss = t.exspList;
    	return t.counts = {
    		ap: ss.filter(e => e.clearstate === 3).length,
    		fc: ss.filter(e => e.clearstate === 2).length,
    		cl: ss.filter(e => e.clearstate === 1).length,
    		nc: ss.filter(e => !e.clearstate).length,
    	}
    }

    function calcUnderGreatCount(s){
    	return (s.great || 0) + (s.good || 0) + (s.bad || 0) + (s.miss || 0)
    }
});
},{"../../js/bndrUtil/bandOrder.js":5,"../../js/bndrUtil/getJsonFromMultiServer.js":8,"../../js/util/storageJson.js":17,"../../js/util/stringFormat.js":18,"jquery":171,"riot":260}],27:[function(require,module,exports){
