riot.tag2('chartlist', '<loading if="{loadingFlag}"></loading><header class="dark"></header><main><cl-options></cl-options><cl-charts></cl-charts></main><footer></footer>', 'chartlist *,[data-is="chartlist"] *,chartlist input,[data-is="chartlist"] input,chartlist select,[data-is="chartlist"] select{ font-family: "Meiryo", -apple-system, BlinkMacSystemFont, "Noto Sans CJK JP", sans-serif; box-sizing: border-box; } chartlist .clearfix:after,[data-is="chartlist"] .clearfix:after{ display: block; clear: both; content: ""; } chartlist .flex-column,[data-is="chartlist"] .flex-column{ display: flex; flex-direction: column; } chartlist .flex-row,[data-is="chartlist"] .flex-row{ display: flex; flex-direction: row; } chartlist *,[data-is="chartlist"] *,chartlist input,[data-is="chartlist"] input,chartlist select,[data-is="chartlist"] select{ font-family: "Meiryo", -apple-system, BlinkMacSystemFont, "Noto Sans CJK JP", sans-serif; box-sizing: border-box; } chartlist .clearfix:after,[data-is="chartlist"] .clearfix:after{ display: block; clear: both; content: ""; } chartlist .flex-column,[data-is="chartlist"] .flex-column{ display: flex; flex-direction: column; } chartlist .flex-row,[data-is="chartlist"] .flex-row{ display: flex; flex-direction: row; } * chartlist,* [data-is="chartlist"]{ background: #111; color: #EEE; display: flex; align-items: center; justify-content: center; flex-direction: column; overflow-y: scroll; } chartlist header,[data-is="chartlist"] header{ position: sticky; top: 0; left: 0; z-index: 1000; } chartlist main,[data-is="chartlist"] main{ display: flex; flex-grow: 1; flex-direction: column; } chartlist cl-options,[data-is="chartlist"] cl-options{ display: flex; position: sticky; flex-direction: row; align-items: center; justify-content: center; width: 100%; z-index: 10000; top: 0; left: 0; background-color: #1A1A1A; } chartlist cl-charts,[data-is="chartlist"] cl-charts{ display: flex; }', '', function(opts) {
    var $ = require("jquery");
    var numeral = require("numeral");
    var storageJson = require("../../js/util/storageJson.js");
    var chartManager = require("../../js/bndrUtil/chartManage.js");
    var calcSum = require("../../js/util/calcArraySum.js");

    this.skillTimeSec = 7.0;
    this.loadingFlag = true;

    this.on("mount", async () => {

    	var chartsObj = await chartManager();
    	this.chartsOrigin = $.extend(true, {}, chartsObj);

    	this.charts = chartsDataSet(chartsObj, {
    		skillTimeSec: this.skillTimeSec,
    	});
    	console.log(this.charts);

    	this.loadingFlag = false;
    	this.update();
    })

    function chartsDataSet(charts, option) {
    	var chartsArr = Object.keys(charts).map(e => charts[e]);
    	return chartsArr.map(c => {
    		var obs = c.chart.objects;
    		var m = c.chart.metadata;

    		var sn = new Array(6).fill(1).map((e,i) => {
    			return obs.filter(e => {
    				return e.effect === "Skill";
    			})[i];
    		});
    		var snts = sn.map(s => {
    			return obs.filter(e => {
    				return e.type !== "System"
    					&& e.timing >= s.timing && e.timing < s.timing + option.skillTimeSec
    			});
    		});
    		var snt = calcSum(snts.map(e => e.length));
    		var snfc = snts.filter((e,i) => {
    			return i >= m.fever.start && i <= m.fever.end;
    		})

    		var FlickNotes = obs.filter(e => {
    			return e.effect === "Flick";
    		}).length;
    		var LongNotes = obs.filter(e => {
    			return e.property === "LongStart";
    		}).length;
    		var SlideNotes = obs.filter(e => {
    			return e.property.includes("Slide");
    		}).length;
    		var NormalNotes = obs.filter(e => {
    			return e.property === "Single" && e.effect !== "Flick";
    		}).length;

    		var lastNote = obs[obs.length - 1];
    		var SongTime = lastNote.timing;
    		var halfNote = obs[Math.round(obs.length / 2) - 1];
    		var HalfNoteTime = halfNote.timing;
    		var HalfNoteRatio = HalfNoteTime / SongTime;

    		c.dataset = {
    			skillNotes: sn,
    			skillNotesEffected: snts,
    			skillNotesEffectedTotal: snt,
    			skillNotesFeverContained: snfc,
    			TotalNotes: m.combo,
    			FeverNotes: m.fever.end - m.fever.start,
    			NormalNotes,
    			FlickNotes,
    			LongNotes,
    			SlideNotes,
    			NormalRatio: (NormalNotes / m.combo),
    			FlickRatio: (FlickNotes / m.combo),
    			LongRatio: (LongNotes * 2 / m.combo),
    			SlideRatio: (SlideNotes / m.combo),
    			SongTime,
    			HalfNoteTime,
    			HalfNoteRatio
    		};
    		return c;
    	});
    }
});
