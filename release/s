: monospace; } htl_autoplay .textOption textarea,[data-is="htl_autoplay"] .textOption textarea{ width: 300px; height: 100px; font-size: smaller; font-family: monospace; } htl_autoplay .resultConsole,[data-is="htl_autoplay"] .resultConsole{ width: 100%; height: 150px; background-color: #1A1A1A; float: left; color: #EEE; padding: 1ex; overflow-y: scroll; font-size: small; } htl_autoplay songsview img.songJacket,[data-is="htl_autoplay"] songsview img.songJacket{ width: 45px; height: 45px; } htl_autoplay cardsview checkutil[val="opts.is_free"],[data-is="htl_autoplay"] cardsview checkutil[val="opts.is_free"]{ display: none; } htl_autoplay cardsview img.cardImage,[data-is="htl_autoplay"] cardsview img.cardImage{ width: 45px; height: 45px; } htl_autoplay calcOptionView,[data-is="htl_autoplay"] calcOptionView{ display: none; }', '', function(opts) {
    const $ = require("jquery");
    const crypto = require("crypto");
    const protobuf = require("protobufjs");
    const hacktimer = require("hacktimer");

    const gbpprt = require("../../data/gbpproto.json");
    const bndrScoreCalc = require("../../js/scorecalc/bndrScoreCalc.js");
    const stringFormat = require("../../js/util/stringFormat.js");
    const accessApi = require("../../js/bndrUtil/getJsonFromMultiServer.js");

    const obs = global.Observer;

    this.on("mount", async () => {

    	this.songsList = await accessApi.getSongs();
    	this.cardsList = await accessApi.getCards([4,3]);
    	this.skillsList = await accessApi.getSkillsList();
    	this.currentEvent = await accessApi.getCurrentEvent();

    	this.update();

    	$(`checkutil[val="opts.is_free"] > input`).trigger("click");

    	obs.trigger("chosenUpdateSongs");
    	obs.trigger("chosenUpdateCards");
    });

    this.onClickRunStart = (async () => {
    	const pickUpMaxNumber = 15;
    	const difficultList = ["easy", "normal", "hard", "expert", "special"];
    	const diffWithoutExSp = ["easy", "normal", "hard"];

    	$(".resultConsole").empty();
    	$("#runstart").prop("disabled", true);

    	var userId = $("#userIdNumber").val() - 0;
    	var headersText = $("#rawtext").val();
    	var headersObj = generateHeadersObj(headersText);
    	var xtoken = $("#xtoken").val();
    	var runmod = $(`input[name="run_type"]:checked`).val() - 0;
    	var sv = this.refs.songs_view;
    	var songData = sv.selectedSong;
    	var songDiffs = sv.selectedSongDifficults;
    	var songId = songData.musicId;
    	var cv = this.refs.cards_view;
    	var setCards = cv.setCards;
    	var bandTotalValue = cv.bandTotalValue || 0;
    	setCards.map(e => {
    		e.skillData = accessApi.getSkillIdDetail(e.skill.skillId, e.skillLevel);
    	});
    	var optv = this.refs.calc_option_view;
    	var calcSetting = optv.setting;
    	calcSetting.isNotCalcBestScore = true;
    	calcSetting.notNeedAbstract = true;

    	if (setCards.length <= 4 || !songData || !bandTotalValue) {

    		return;
    	}

    	var targetDiff = [];
    	switch (runmod){
    		case 1:
    			targetDiff = [sv.selectedDifficult];
    			break;
    		case 2:
    			targetDiff = diffWithoutExSp;
    			break;
    		case 3:
    			targetDiff = difficultList;
    			break;
    	}

    	insertLog(`曲名:【 ${songData.title} 】`);
    	for(var songdiff of targetDiff) {

    		var chartData = await accessApi.getChart(songId, songdiff);
    		chartData.objects = chartData.objects.filter(e => e.type !== "System");
    		var songLevel = songDiffs.find(e => e.diff_l.includes(songdiff)).level;
    		var scoreCalcObj = {
    			"Observer": obs,
    			"songChartData": chartData,
    			"setting": calcSetting,
    			"eventData": this.currentEvent,
    			"songDifficult": songdiff,
    			setCards, bandTotalValue,
    			songData, songLevel,
    		}
    		var scoreCalcRst = await bndrScoreCalc.scoreCalculate(scoreCalcObj);

    		var pn = Math.floor(Math.random() * pickUpMaxNumber);
    		var pickUpResult = scoreCalcRst.calculatedScores[pn];
    		var requestObjectOfMusicResult = bndrScoreCalc.generateReuseCode(scoreCalcObj, {
    			"totalScore": pickUpResult.scoreOnAllJust,
    			"notesDetail": pickUpResult.notesDetail,
    			"pattern": pickUpResult.pattern
    		}).musicResultObj;

    		var emptyFavStateMap = [];
    		var requestObjectOfMusicPre = {
    			"liveType": "free_live",
    			"liveBoostUseCount": 1,
    			"musicIdFavoriteStateMap": emptyFavStateMap,
    		};
    		xtoken = await sendRequestToGbpServer({
    			url: `/api/user/${userId}/music/${songId}/`,
    			headers: headersObj,
    			xtoken: xtoken,
    			method: "POST",
    			dataFormat: "UserMusicPreProcessRequest",
    			data: requestObjectOfMusicPre,
    		});

    		var nd = pickUpResult.notesDetail;
    		var lastNote = nd[nd.length - 1];
    		var wt = lastNote.timing + 10;
    		insertLog(`[${songdiff}]のプレイ開始… ${wt}秒待機します`);
    		await waitTime(wt);

    		xtoken = await sendRequestToGbpServer({
    			url: `/api/suite/user/${userId}/music/${songId}/`,
    			headers: headersObj,
    			xtoken: xtoken,
    			method: "PUT",
    			dataFormat: "UserMusicRequest",
    			data: requestObjectOfMusicResult,
    		});

    		insertLog(`[${songdiff}]のプレイ修了… 20秒待機します`);
    		await waitTime(20);
    	};

    	$("#runstart").prop("disabled", false);
    	$("#xtoken").val(xtoken);
    	insertLog("--- finish ---");

    }).bind(this);

    async function sendRequestToGbpServer(opts){
    	const listUpHeadersName = [
    		"X-Unity-Version", "Accept", "X-Signature",
    		"X-MasterDataVersion", "X-ClientVersion",
    		"X-DataVersion", "X-ClientPlatform",
    		"Accept-Language", "Accept-Encoding",
    		"User-Agent", "Connection", "Content-Type",
    	];
    	const replacedHeadersList = [
    		"User-Agent", "Connection", "Accept-Encoding"
    	];
    	var {url, xtoken, data, dataFormat, method, headers} = opts;
    	var root = protobuf.Root.fromJSON(gbpprt);
    	var protoType = root.lookupType(dataFormat);

    	var pbMsg = protoType.create(data);
    	var encodeUintArray = protoType.encode(pbMsg).finish();

    	var encryptedData = encryptWithAES(encodeUintArray);

    	var preHeader = {};
    	listUpHeadersName.forEach(key => {
    		var nk = key;
    		if(replacedHeadersList.includes(key)){
    			nk = "Replaced-" + key;
    		}
    		preHeader[nk] = headers[key];
    	});
    	preHeader["X-Token"] = xtoken;

    	var rst = await new Promise((res, rej) => {
    		try {
    			var oReq = new XMLHttpRequest();
    			var targetUrl = `https://gbpa.glitch.me${url}`;
    			oReq.open(method, targetUrl, true);
    			oReq.onload = function(){
    				res(oReq);
    			};
    			oReq.onerror = function(e){
    				insertLog(`ERROR OCCURED in data-send process.`);
    				rej(e);
    			};
    			for(var key of Object.keys(preHeader)){
    				oReq.setRequestHeader(key, preHeader[key]);
    			}
    			oReq.send(encryptedData);
    		} catch(e) {
    			insertLog(e.replace(/\n/g, "<br/>"));
    			rej(e);
    		}
    	});

    	xtoken = rst.responseText;
    	return xtoken;
    }

    function encryptWithAES(bytes){
    	const skey = "mikumikulukaluka";
    	const siv = "lukalukamikumiku";

    	var cipher = crypto.createCipheriv('aes-128-cbc', skey, siv);
    	var enc = cipher.update(bytes);
    	enc = concatenation([enc, cipher.final()]);

    	return enc;

    }

    function generateHeadersObj(texts) {
    	var obj = {};
    	var headers = texts.split("\n").splice(2);
    	for(var h of headers){
    		if(h === "") { break; }
    		var s = h.split(":");
    		obj[s[0]] = s[1].trim();
    	}
    	return obj;
    }

    function insertLog(text) {
    	$(".resultConsole").append(`<div class="console">${text}</div>`);
    	$(".resultConsole > .console:last-child")[0].scrollIntoView(true);
    	console.log(text);
    }

    function waitTime(sec) {
    	return new Promise(resolve => setTimeout(resolve, sec * 1000))
    }

    function concatenation(segments){
    	var sumLength = 0;
    	for (var i = 0; i < segments.length; ++i) {
    		sumLength += segments[i].byteLength;
    	}
    	var whole = new Uint8Array(sumLength);
    	var pos = 0;
    	for (var i = 0; i < segments.length; ++i) {
    		whole.set(new Uint8Array(segments[i]), pos);
    		pos += segments[i].byteLength;
    	}
    	return whole.buffer;
    }
});
}).call(this,typeof global !== "undefined" ? global : typeof self !== "undefined" ? self : typeof window !== "undefined" ? window : {})
},{"../../data/gbpproto.json":1,"../../js/bndrUtil/getJsonFromMultiServer.js":8,"../../js/scorecalc/bndrScoreCalc.js":12,"../../js/util/stringFormat.js":18,"crypto":119,"hacktimer":151,"jquery":171,"protobufjs":202,"riot":260}],41:[function(require,module,exports){
var riot = require('riot');
module.exports = 
riot.tag2('eventsview', '<loading if="{isLoading}"></loading><header class="dark"></header><main><table><thead><tr><td>ID</td><td>event name</td><td>span</td><td>attr</td><td>type</td><td>gacha[1]</td><td>gacha[2]</td></tr></thead><tbody><tr class="event" each="{e in events}"><td class="id">{e.id}</td><td class="name"><a href="{e.url}" target="_blank">{e.name}</a></td><td class="span">{e.span}</td><td class="attr {e.attr}">{e.attrJp}</td><td class="type {e.type}">{e.typeJp}</td><td class="gdesc">{e.gdesc[0]}</td><td class="gdesc">{e.gdesc[1]}</td></tr></tbody></table></main><footer class="dark"></footer>', '* eventsview,* [data-is="eventsview"]{ display: flex; flex-direction: column; background-color: #000; color: #EEE; } eventsview main,[data-is="eventsview"] main{ display: flex; flex-direction: row; padding: 1ex; max-width: 1000px; margin: 0 auto; flex-grow: 1; } eventsview .event,[data-is="eventsview"] .event{ align-items: center; border-bottom: 1px solid #888; background: #000; align-items: center; padding: .7ex; font-size: 0.85rem; } eventsview .event:nth-child(even),[data-is="eventsview"] .event:nth-child(even){ background: #111; } eventsview .event:last-child,[data-is="eventsview"] .event:last-child{ border-bottom: 0px; } eventsview .event td,[data-is="eventsview"] .event td{ padding: .8ex .5ex; text-align: center; color: #FFF; } eventsview td.name,[data-is="eventsview"] td.name{ text-align: left; } eventsview td.gdesc,[data-is="eventsview"] td.gdesc,eventsview td.id,[data-is="eventsview"] td.id{ text-align: right; } eventsview td > a,[data-is="eventsview"] td > a{ color: #0BF; } eventsview thead,[data-is="eventsview"] thead{ font-family: "Iceland"; text-transform: uppercase; text-align: center; font-size: 0.8rem; background: #06A; color: #FFF; padding: 1ex; }', '', function(opts) {
    var $ = require("jquery");
    var numeral = require("numeral");
    var date_utils = require("date-utils");
    var accessApi = require("../../js/bndrUtil/getJsonFromMultiServer.js");

    this.on("mount", async () => {
    	var evs = await accessApi.getEvents();
    	var gcs = await accessApi.getGachaes();

    	this.eventList = Object.values(evs);
    	this.gachaList = Object.values(gcs);
    	this.cards = (await accessApi.getCards([4])).data;
    	this.names = await accessApi.getNames();

    	this.events = formatEventList(this);
    	console.log(this.events);

    	this.update();
    });

    function formatEventList(t) {
    	const eventTypeJp = {
    		"story": "ストーリー",
    		"challenge": "チャレンジ",
    		"versus": "対バン",
    		"live_try": "トライ",
    		"mission_live": "ミッション"
    	};
    	const attrJp = {
    		"cool": "クール",
    		"happy": "ハッピー",
    		"pure": "ピュア",
    		"powerful": "パワフル",
    	}

    	return t.eventList.map((e,i) => {
    		var startDate = e.startAt[0];
    		var gacha = t.gachaList.find(g => g.publishedAt[0] === startDate && g.newCards.length > 0);

    		var startDateStr = new Date(startDate - 0).toFormat("YY/MM/DD");
    		var endDate = e.endAt[0];
    		var endDateStr = new Date(endDate - 0).toFormat("MM/DD");

    		var attr = e.attributes[0].attribute;
    		var newCards = gacha ? gacha.newCards : [];
    		var gachaCards = pickUpCards(t.cards, newCards, startDate);
    		var distCards = pickUpCards(t.cards, e.rewardCards, startDate);
    		var gachaDesc = generateCardDesc(gachaCards, t.names, true);
    		var charas = e.characters.map(c => c.characterId - 0);
    		var charaFirstNames = charas.map(cid => {
    			return t.names.find(n => n.id === cid).first;
    		}).join("/");

    		return {
    			id: i + 1,
    			url: `https://bestdori.com/info/events/${i+1}/`,
    			attr,
    			attrJp: attrJp[attr],
    			charas,
    			charastr: charaFirstNames,
    			start: startDate,
    			end: endDate,
    			span: `${startDateStr} - ${endDateStr}`,
    			sfmt: startDateStr,
    			efmt: endDateStr,
    			name: e.eventName[0],
    			type: e.eventType,
    			typeJp: eventTypeJp[e.eventType],
    			gachas: gachaCards,
    			gdesc: gachaDesc,
    			dists: distCards,
    		};
    	}).filter(e => e).sort((a, b) => {
    		return (b.start - 0) - (a.start - 0);
    	});
    }

    function pickUpCards(cards, ids, publishDate){
    	return ids.map(id => {
    		var c = cards.find(e => e.cardId === id && e.releasedAt === publishDate);
    		if(!c){ return null; }
    		c.skillDetail = accessApi.getSkillIdDetail(c.skill.skillId, 5);
    		return c;
    	}).filter(e => e);
    }

    function generateCardDesc(cards, names, withoutP){
    	return cards.map(c => {
    		var d = c.skillDetail;
    		var charaId = c.characterId;
    		var first = names.find(e => e.id == charaId).first;
    		var skillRate = d.scoreUpRate;
    		var isP = !d.isContinuedPerfectOnly && d.isPerfectOnly;
    		var P = isP ? "P" : "";
    		var G = d.isContinuedPerfectOnly ? "G" : "";
    		if(withoutP && isP){ return null; }
    		return {
    			score: skillRate,
    			format: `${first} [${skillRate}${P}${G}]`,
    		};
    	}).filter(e => e).sort((a,b) => {
    		return b.score - a.score;
    	}).map(e => {
    		return e.format;
    	});
    }
});
