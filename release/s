(function (global){
// API Host URLs
var CORSP = "https://cors-anywhere.herokuapp.com/";
var BDR_PURL= "https://bestdori.com";
var BDR_URL = "" + CORSP + BDR_PURL + "/api";
var BDB_URL = global.APIROOT;

// urls(bestdori)
var BDR_Chart = BDR_URL + "/songs/chart/graphics/chart/{0}.{1}.json";
var BDR_Cards = BDR_URL + "/cards/all.5.json";
var BDR_Skills = BDR_URL + "/skills/all.5.json";
var BDR_Songs = BDR_URL + "/songs/all.7.json";
var BDR_Names = BDR_URL + "/characters/main.3.json";
var BDR_Events = BDR_URL + "/events/all.6.json";
var BDR_EvDetail = BDR_URL + "/events/{0}.json";

// urls(bandori database)
var BDB_Chart = BDB_URL + "/music/chart/{0}/{1}";
var BDB_Cards = BDB_URL + "/card?";
var BDB_Skills = BDB_URL + "/skill";
var BDB_Songs = BDB_URL + "/music";
var BDB_Names = BDB_URL + "/chara/band";
var BDB_Event = BDB_URL + "/event";

// other setting

var $ = require("jquery");
var numeral = require("numeral");
var storageFile = require("../util/storageFile.js");
var stringFormat = require("../util/stringFormat.js");
var skillJson = require("../../data/skill.json");

module.exports = {
	getChart: async function (songId, difficult){
		var url_db = stringFormat(BDB_Chart, songId, difficult);
		var url_dr = stringFormat(BDR_Chart, songId, difficult);
		var cs = await BDBandBDRapiManager(url_db, url_dr, {
			dr_callback: convertBDR2BDBchart,
			dr_cbOpts: { songId: songId, difficult: difficult },
			db_errCheck: function (rst) {
				return rst && rst.objects && rst.objects.length > 0;
			},
		});
		return cs;
	},
	
	getCards: async function(rarity) {
		if ( rarity === void 0 ) rarity = [4,3];

		var BDB_Imgset = function (rst, opts) {
			rst.data.forEach(function (e) {
				e.imageUrlParentDir = (global.RESROOT) + "/assets-jp";
			});
			return rst;
		};
		var BDRtoBDB = function (rst, opts) {
			var data = Object.keys(rst).map(function (k) {
				var c = rst[k];
				var attribute = c.attribute;
				var characterId = c.characterId;
				var rarity = c.rarity;
				var skillId = c.skillId;
				var calcedMaxLv = (rarity+1) * 10 + (rarity >= 3 ? 10 : 0);
				var isAvaiable = !!c.prefix[0];
				if(!isAvaiable){ return null; }
				
				var imageUrlParentDir = BDR_PURL + "/assets/jp";
				var gacha = ["permanent", "limited"].includes(c.type) ? c.type : undefined;
				return {
					characterId: characterId, rarity: rarity, skillId: skillId, imageUrlParentDir: imageUrlParentDir,
					cardId: k - 0,
					attr: attribute,
					cardRes: c.resourceSetName,
					title: c.prefix[0],
					releasedAt: c.releasedAt[0],
					skill: {skillId: skillId},
					gachaText: gacha,
					distType: c.type,
					simpleParams: {
						min: c.stat["1"],
						max: c.stat[("" + calcedMaxLv)],
					},
				}
			}).filter(function (e) { return !!e && rarity.includes(e.rarity); });
			return {data: data};
		};
		var db_cards = BDB_Cards + rarity.map(function (e) { return ("rarity[]=" + e); }).join("&");
		var cs = await BDBandBDRapiManager(db_cards, BDR_Cards, {
			db_callback: BDB_Imgset,
			dr_callback: BDRtoBDB
		});
		return {data: cs.data};
	},
	
	getSongs: async function() {
		var BDRtoBDB = function (rst) {
			var rstObj = Object.keys(rst).map(function (k) {
				var calcJacketURLPath = function (id, image) {
					var idceil10 = Math.ceil(id/10)*10;
					var dir = "/musicjacket/musicjacket" + idceil10 + "_rip/";
					var filepre = "assets-star-forassetbundle-startapp-musicjacket-";
					var file = filepre + "musicjacket" + idceil10 + "-" + image + "-jacket.png";
					return dir + file;
				};
				var v = rst[k];
				var musicId = k - 0;
				var bandId = v.bandId;
				var tag = v.tag;
				var jacketImage = v.jacketImage[0];
				// var jacket = `/musicjacket/${jacketImage}_rip/jacket.png`;
				var jacket = calcJacketURLPath(musicId, jacketImage);
				var imageUrlParentDir = BDR_PURL + "/assets/jp";
				var bgmFile = "/assets/jp/sound/bgm" + musicId + "_rip/bgm" + musicId + ".mp3";
				var bgmId = "bgm" + musicId;
				var difficulty = [0, 3, 2, 1, 4].map(function (i) {
					var e = v.difficulty[i];
					return e ? e.playLevel : undefined;
				}).filter(function (e) { return e >= 0; });
				var maxDifficulty = difficulty[1];
				
				return {
					musicId: musicId, bandId: bandId, tag: tag, jacket: jacket, jacketImage: jacketImage,
					bgmFile: bgmFile, bgmId: bgmId, difficulty: difficulty,
					imageUrlParentDir: imageUrlParentDir,
					title: v.musicTitle[0],
					chartAssetBundleName: jacketImage,
				};
			}).filter(function (e) {
				return !!e.title;
			}).reverse();
			return {data: rstObj};
		};
		var BDB_fullNameChange = function (rst) {
			rst.data = rst.data.map(function (e) {
				e.title = e.title.replace(/甅/g, "[FULL] ");
				e.imageUrlParentDir = "" + (global.RESROOT);
				return e;
			});
			return rst;
		};
		return await BDBandBDRapiManager(BDB_Songs, BDR_Songs, {
			dr_callback: BDRtoBDB,
			db_callback: BDB_fullNameChange,
		});
	},
	
	getNames: async function() {
		var BDRfix = function (rst) {
			return Object.keys(rst).map(function (i) {
				var v = rst[i];
				return {
					id: i - 0,
					name: v.characterName[0],
					first: v.firstName[0],
				}
			});
		};
		var BDBfix = function (rst) {
			return rst.map(function (e) {
				return {
					id: e.characterId,
					name: e.characterName,
					first: e.firstName,
				}
			})
		};
		
		return await BDBandBDRapiManager(BDB_Names, BDR_Names, {
			dr_callback: BDRfix,
			db_callback: BDBfix,
		});
	},
	
	getCurrentEvent: async function() {
		var BDRtoBDB = function (rst) {
			return {
				eventType: rst.eventType,
				eventName: rst.eventName[0],
				assetBundleName: rst.assetBundleName,
				bannerAssetBundleName: rst.bannerAssetBundleName,
				startAt: rst.startAt[0],
				endAt: rst.endAt[0],
				publicStartAt: rst.publicStartAt[0],
				publicEndAt: rst.publicEndAt[0],
				distributionStartAt: rst.distributionStartAt[0],
				distributionEndAt: rst.distributionEndAt[0],
				bgmAssetBundleName: rst.bgmAssetBundleName,
				bgmFileName: rst.bgmFileName,
				aggregateEndAt: rst.aggregateEndAt[0],
				exchangeEndAt: rst.exchangeEndAt[0],
				pointRewards: rst.pointRewards,
				rankingRewards: rst.rankingRewards,
				stories: rst.stories,
				rewardCards: rst.rewardCards,
				detail: {
					musics: rst.musics ? rst.musics[0] : undefined,
					attributes: rst.attributes,
					characters: rst.characters,
					paramBonus: [rst.eventCharacterParameterBonus],
				}
			};
		};
		var getCurrentEvent = async function(events_bdr) {
			// get current event id
			var latestEventId = Object.keys(events_bdr).map(function (k) {
				var v = events_bdr[k];
				var startDate = new Date(v.startAt[0] - 0);
				return {k: k, startDate: startDate};
			}).sort(function (a,b) {
				return b.startDate - a.startDate;
			})[0].k;
			
			var BDR_Event = stringFormat(BDR_EvDetail, latestEventId);
			var detail = await $.getJSON(BDR_Event);
			return BDRtoBDB(detail);
		};
		
		return await BDBandBDRapiManager(BDB_Event, BDR_Events, {
			dr_callback: getCurrentEvent
		});
	},
	
	// bestdori only -----
	getEvents: async function() {
		return await $.getJSON((BDR_URL + "/events/all.6.json"));
	},
	
	getGachaes: async function() {
		return await $.getJSON((BDR_URL + "/gacha/all.5.json"));
	},
	
	// no ajax -----
	getSkillsList: async function() {
		return skillJson;
	},
	
	getSkillIdDetail: function(id, slv) {
		return getSkillIdDetail(id, slv);
	},
};

async function BDBandBDRapiManager(db_url, dr_url, options){
	var blankFunc = function async (t) { return t; };
	var apiAccess = async function (opt) {
		var url = opt.url;
		var callback = opt.callback;
		var callbackOpts = opt.callbackOpts;
		var errorCheck = opt.errorCheck;
		var j = await getJson(url);
		var cbk = callback || blankFunc;
		var eck = errorCheck || (function (e) { return e; });
		var c = await cbk(j, callbackOpts || {});
		var ec = eck(c);
		return ec ? c : null;
	};
	var multiApiAccess = async function (opts) {
		var after_cb = options.after_callback || (function (e) { return e; });
		for(var i = 0; i < opts.length; i++){
			var opt = opts[i];
			var r = await apiAccess(opt);
			if(!!r){
				return after_cb(r);
			}
		}
		return null;
	};
	var getJson = function (url) {
		return new Promise(function (res, rej) {
			$.getJSON(url)
				.then(function (data) { return res(data); })
				.fail(function (err) { return res(null); });
		});
	};

	var b_rs, r_rs, rst;
	var dra = {
		url: dr_url,
		callback: options.dr_callback,
		callbackOpts: options.dr_cbOpts,
		errorCheck: options.dr_errCheck,
	};
	var dba = {
		url: db_url,
		callback: options.db_callback,
		callbackOpts: options.db_cbOpts,
		errorCheck: options.db_errCheck,
	};
	
	if(global.IS_PRIORITY_BDR){
		rst = await multiApiAccess([dra, dba]);
	} else {
		rst = await multiApiAccess([dba, dra]);
	}
	
	return rst;
}

function convertBDR2BDBchart(json, opts){
	var id = opts.songId;
	var isLNnote = function (n) {
		return n.type.includes("Long") || n.type.includes("Tick");
	};
	
	// 最初に拍情報を足す
	var jsb = addBeatInfoFromBPM(json);
	// グラフィック関連の情報を除く(ただしLN棒は後で使う)
	//   Sim: simultaneous pressing, Bar: LN Bar
	var rg1 = jsb.filter(function (e) { return ["Sim", "Bar"].every(function (f) { return !e.type.includes(f); }); });
	var rgB = jsb.filter(function (e) { return ["Bar"].every(function (f) { return e.type.includes(f); }); });
	// LNの情報を付与する、主にAB情報
	rg1.forEach(function (e) {
		// すでに処理済ならスルー
		if(e.side || e.isend){
			return false;
		}
		// LN情報を取得
		var bi = rgB.find(function (b) { return b.time[0] === e.time && b.lane[0] === e.lane; });
		if(!bi){
			return false;
		}
		e.islong = true;
		
		var isSameLane = bi.lane[1] === bi.lane[0];
		var lnEndNote = rg1.find(function (g) { return g.time === bi.time[1] && g.lane === bi.lane[1]; });
		var isNotNextTick = !lnEndNote.type.includes("Tick");
		var isLong = isSameLane && isNotNextTick;
		if(isLong){
			// LNなら(スライドでないなら)
			// LN終わりのノーツに情報を付与する
			lnEndNote.isend = true;
		} else {
			// スライドの場合
			// すでにAを使っている場合はB、それ以外はA
			var nearLNNoteWithSide = rg1.find(function (g) { return g.time >= e.time && isLNnote(g) && g.side; });
			var useSide = (!nearLNNoteWithSide || nearLNNoteWithSide.side.includes("B")) ? "A" : "B";
			// 支点ノーツに指定
			e.side = useSide;
			e.isslide = true;
			
			// LNが終わるまで順番に辿っていく
			var lt = null;
			while(bi){
				// 次のLong/Tickを探す
				lt = rg1.find(function (g) { return g.time === bi.time[1] && g.lane === bi.lane[1]; });
				if(!lt){ break; }
				lt.side = useSide;
				lt.isslide = true;
				// 見つけた箇所が始点になるLNを探す
				bi = rgB.find(function (b) { return b.time[0] === lt.time && b.lane[0] === lt.lane; });
			}
			if(lt){
				// 最後に見つかった箇所を終点扱いにする
				lt.isend = true;
			}
		}
	});
	
	// convert to BDB chart data
	var objects = rg1.map(function (e) {
		var type = "Object";
		var effect = "";
		var property = "Single";
		var beat = e.beats;
		var lane = e.lane;
		var timing = e.time;
		var value = undefined;
		
		var se = e.isend ? "End" : "Start";
		switch(e.type){
			case "Single":
			case "SingleOff":
				// 通常ノーツ
				effect = "Single";
				break;
			case "Flick":
				// LN関連
				effect = "Flick";
				if(e.isslide && e.isend) {
					effect = "SlideEndFlick_" + (e.side);
					property = "Slide";
				} else if(e.isend){
					property = "Long" + se;
				}
				break;
			case "Skill":
				// スキルノーツ
				effect = "Skill";
				if(e.islong){
					property = "Long" + se;
				}
				break;
			case "Long":
				// LN関連
				if(e.isslide){
					effect = "Slide" + se + "_" + (e.side);
					property = "Slide";
				} else {
					effect = "Single";
					property = "Long" + se;
				}
				break;
			case "Tick":
				// LN中点
				effect = "Slide_" + (e.side);
				property = "Slide";
				break;
			case "BPM":
				// BPM変動
				effect = "BPMChange";
				property = "Special";
				type = "System";
				value = e.bpm;
				break;
			default:
				// それ以外(フィーバー関連)
				effect = "Cmd" + (e.type);
				property = "Special";
				type = "System";
				break;
		}
		var obj = {
			effect: effect, property: property, type: type, beat: beat, lane: lane, timing: timing, value: value,
		};
		return JSON.parse(JSON.stringify(obj));
	});
	
	var metadata = {
		bgm: ("bgm" + (numeral(id).format("000")) + ".wav"),
		bpm: objects.find(function (e) { return e.effect.includes("BPMChange"); }).value,
		combo: objects.filter(function (e) { return !e.type.includes("System"); }).length,
	};
	
	return {
		metadata: metadata, objects: objects
	}
}

function addBeatInfoFromBPM(json) {
	var changedTiming = 0;
	var changedBeats = 0;
	var nowBPM = 100;
	var convertBeat = function (time, bpm) {
		return time * bpm / 60;
	};
	json.forEach(function (e, i) {
		var cbb = changedBeats + convertBeat(e.time - changedTiming, nowBPM);
		var cb = numeral(cbb).format("0.000000") - 0;
		if (e.type.includes("BPM")) {
			// setting update
			changedBeats = cb;
			changedTiming = e.time;
			nowBPM = e.bpm;
		}
		// beats info added
		e.beats = cb;
	});
	return json;
}


function getSkillIdDetail(id, slv){
	var SkillDurationTypeA = [5.0,5.5,6.0,6.5,7.0];
	var SkillDurationTypeB = [5.0,5.7,6.4,7.2,8.0];
	var SkillDurationTypeC = [5.0,5.6,6.2,6.8,7.5];
	
	var durations;
	// skill duration categorise
	switch(id) {
		case 5:
		case 6:
		case 7:
		case 8:
		case 9:
		case 10:
			// 判定orライフ回復(特大)
			durations = SkillDurationTypeB;
			break;
		case 11:
		case 12:
		case 13:
		case 14:
		case 15:
		case 16:
			// 判定orライフ回復(大)
			durations = SkillDurationTypeC;
			break;
		default:
			durations = SkillDurationTypeA;
			break;
		
	};
	var scoreDuration = durations[slv - 1];
	
	// skill score-up and judge info
	var sJson = skillJson.find(function (e) { return e.skillId === id; });
	var conditionBandId = sJson.skillConditionOfBandId,
		conditionAttr = sJson.skillConditionOfAttr;
	var scoreUpRate, scoreUpRate2 = 0,
		scoreUpRateWOB = 0, scoreUpRate2WOB = 0,
		isContainJudgeEffect = false, judgeTargetCondition = "",
		isPerfectOnly = false, isContinuedPerfectOnly = false;
		
	switch(id) {
		case 1:
			// スコア10%ＵＰ
			scoreUpRate = 10;
			break;
		case 2:
			// スコア30%ＵＰ
			scoreUpRate = 30;
			break;
		case 3:
			// スコア60%ＵＰ
			scoreUpRate = 60;
			break;
		case 4:
			// スコア100%ＵＰ
			scoreUpRate = 100;
			break;
		case 5:
			// 判定強化(中)＆スコア10%ＵＰ
			scoreUpRate = 10;
			isContainJudgeEffect = true;
			judgeTargetCondition = "great";
			break;
		case 6:
			// 判定強化(大)＆スコア20%ＵＰ
			scoreUpRate = 20;
			isContainJudgeEffect = true;
			judgeTargetCondition = "good";
			break;
		case 7:
			// 判定強化(特大)＆スコア40%ＵＰ
			scoreUpRate = 40;
			isContainJudgeEffect = true;
			judgeTargetCondition = "bad";
			break;
		case 8:
			// ライフ300回復＆スコア10%ＵＰ
			scoreUpRate = 10;
			break;
		case 9:
			// ライフ450回復＆スコア20%ＵＰ
			scoreUpRate = 20;
			break;
		case 10:
			// ライフ750回復＆スコア40%ＵＰ
			scoreUpRate = 40;
			break;
		case 11:
			// 判定強化(中)＆スコア30%ＵＰ
			scoreUpRate = 30;
			isContainJudgeEffect = true;
			judgeTargetCondition = "great";
			break;
		case 12:
			// 判定強化(大)＆スコア60%ＵＰ
			scoreUpRate = 60;
			isContainJudgeEffect = true;
			judgeTargetCondition = "good";
			break;
		case 13:
			// ライフ300回復＆スコア30%ＵＰ
			scoreUpRate = 30;
			break;
		case 14:
			// ライフ450回復＆スコア60%ＵＰ
			scoreUpRate = 60;
			break;
		case 15:
			// ライフ300回復＆判定強化(中)
			scoreUpRate = 0;
			isContainJudgeEffect = true;
			judgeTargetCondition = "great";
			break;
		case 16:
			// ライフ450回復＆判定強化(大)
			scoreUpRate = 0;
			isContainJudgeEffect = true;
			judgeTargetCondition = "good";
			break;
		case 17:
			// ライフ900以上でスコア65%UP|ライフ900未満の場合スコア55%UP
			scoreUpRate = 65;
			break;
		case 18:
			// ライフ900以上でスコア110%UP|ライフ900未満の場合スコア90%UP
			scoreUpRate = 110;
			break;
		case 20:
			// PERFECTのみスコア120%UP
			scoreUpRate = 120;
			isPerfectOnly = true;
			break;
		case 21:
			// ライフ600以上でスコア40%UP|ライフ600未満の場合ライフ450回復
			scoreUpRate = 40;
			break;
		case 22:
			// ライフ600以上でスコア80%UP|ライフ600未満の場合ライフ500回復
			scoreUpRate = 80;
			break;
		case 23:
			// ライフ減少無効＆スコア10％UP
			scoreUpRate = 10;
			break;
		case 24:
			// ライフ減少無効＆スコア30％UP
			scoreUpRate = 30;
			break;
		case 25:
			// GREAT以下を出すまでスコアが65％UP
			scoreUpRate = 65;
			scoreUpRate2 = 55;
			isPerfectOnly = true;
			isContinuedPerfectOnly = true;
			break;
		case 26:
			// GREAT以下を出すまでスコアが110％UP
			scoreUpRate = 110;
			scoreUpRate2 = 90;
			isPerfectOnly = true;
			isContinuedPerfectOnly = true;
			break;
		case 27: // (band)poppin party
		case 28: // (band)afterglow
		case 29: // (band)pastel*palette
		case 30: // (band)roselia
		case 31: // (band)hello happy world
		case 32: // (band)morfonica
		case 33: // (type)happy
		case 34: // (type)cool
		case 35: // (type)powerful
		case 36: // (type)pure
			// PERFECTのみ110%UP|(バンド指定)の場合はPERFECTのみ125%UP
			scoreUpRate = 125;
			scoreUpRateWOB = 110;
			isPerfectOnly = true;
			break;
		case 37: // (band)poppin party
		case 38: // (band)afterglow
		case 39: // (band)pastel*palette
		case 40: // (band)roselia
		case 41: // (band)hello happy world
		case 42: // (band)morfonica
		case 43: // (type)happy
		case 44: // (type)cool
		case 45: // (type)powerful
		case 46: // (type)pure
			// GREAT以下を出すまで95％UP|(バンド指定)の場合はGREAT以下を出すまで115%UP
			scoreUpRate = 115;
			scoreUpRateWOB = 95;
			scoreUpRate2 = 80;
			scoreUpRate2WOB = 80;
			isPerfectOnly = true;
			isContinuedPerfectOnly = true;
			break;
		case 47: // (band)poppin party
		case 48: // (band)afterglow
		case 49: // (band)pastel*palette
		case 50: // (band)roselia
		case 51: // (band)hello happy world
		case 52: // (band)morfonica
		case 53: // (type)happy
		case 54: // (type)cool
		case 55: // (type)powerful
		case 56: // (type)pure
			// 80％UP|(バンド指定)の場合は105%UP
			scoreUpRate = 105;
			scoreUpRateWOB = 80;
			break;
		default:
			// UNKNOWN SKILL
			console.error(("UNKNOWN SKILL: " + id));
			break;
	};
	
	return {
		lv: slv,
		scoreUpRate: scoreUpRate, scoreUpRate2: scoreUpRate2,
		scoreUpRateWOB: scoreUpRateWOB, scoreUpRate2WOB: scoreUpRate2WOB,
		scoreDuration: scoreDuration, isContainJudgeEffect: isContainJudgeEffect, judgeTargetCondition: judgeTargetCondition,
		isPerfectOnly: isPerfectOnly, isContinuedPerfectOnly: isContinuedPerfectOnly,
		conditionBandId: conditionBandId, conditionAttr: conditionAttr,
	}
	
}
