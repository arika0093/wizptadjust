    var stringFormat = require("../../js/util/stringFormat.js");
    var $ = require("jquery");
    window.jQuery = window.$ = $;
    var chosen = require("chosen-js");
    var obs = global.Observer;

    this.on("mount", () => {

    })

    obs.on("chosenUpdateSongs", () => {

    	$(`#songsList`).chosen({
    		width: "300px"
    	}).change(e => {
    		(onSelectSong.bind(this))(e);
    		this.update();
    	});
    })

    function onSelectSong(e) {
    	const DifficultName = ["Easy", "Normal", "Hard", "Expert", "Special"];

    	var seledOption = e.target.selectedOptions[0];
    	var songIndex = seledOption.value;
    	if(songIndex < 0){
    		return;
    	}
    	var seledSong = this.selectedSong = opts.songs.data[songIndex];

    	var [esl, exl, hdl, nrl, spl] = seledSong.difficulty;
    	var songDifficults = [esl, nrl, hdl, exl, spl].map((e,i) => {
    		if(!e){
    			return undefined;
    		}
    		return {
    			difficult: DifficultName[i],
    			diff_l: DifficultName[i].toLowerCase(),
    			level: e
    		}
    	}).filter(e => !!e);
    	songDifficults.sort((a,b) => b.level - a.level);
    	this.selectedSongDifficults = songDifficults;
    	this.selectedDifficult = songDifficults[0].diff_l;
    	this.selectedDifficultLevel = songDifficults[0].level;

    	var parDir = seledSong.imageUrlParentDir || global.RESROOT;
    	this.songJacketUrl = parDir + seledSong.jacket;

    }

    this.onSelectDifficult = function(e) {
    	var seledOption = e.target.selectedOptions[0];
    	this.selectedDifficult = seledOption.value;
    	this.selectedDifficultLevel = seledOption.dataset.level;

    }.bind(this)

    this.isEventSong = function(song) {
    	var flag = false
    	var event = opts.event;
    	var ev_songs = event.detail.musics;
    	if(ev_songs){
    		return ev_songs.map(e => e.musicId).indexOf(song.musicId) >= 0;
    	}
    	return flag;
    }.bind(this)
});
}).call(this,typeof global !== "undefined" ? global : typeof self !== "undefined" ? self : typeof window !== "undefined" ? window : {})
