    	if(this.setCards.length > 0){
    		this.bandTotalValue = calcTotal.calcTotalValuesFromOpts(this.setCards, opts);
    		this.update();
    	}
    }.bind(this)

    this.onChangeACM =async  function(e) {
    	const AutoCollect = 999;
    	var bandValue = $("#acmBand").val();
    	var attrValue = $("#acmAttr").val();

    	if(!opts.cards){

    		return;
    	}
    	var cards = opts.cards.data;
    	var characters = [];
    	var attributes = "";

    	if(bandValue < 0 || attrValue == -1){

    		return;
    	}
    	if(bandValue == AutoCollect || attrValue == AutoCollect){

    		if(bandValue == AutoCollect){
    			characters = opts.event.detail.characters.map(e => e.characterId);
    		}
    		if(attrValue == AutoCollect){
    			attributes = opts.event.detail.attributes[0].attribute;
    		}
    	}
    	if(characters.length <= 0 && bandValue >= 0 && bandValue != AutoCollect){
    		characters = [1,2,3,4,5].map(e => 5 * bandValue + e);
    	}
    	if(!attributes){
    		attributes = attrValue;
    	}

    	characters.forEach((charaId, i) => {
    		var pickCard = cards.filter(e => {

    			return e.characterId == charaId && e.title.indexOf("ガルパ杯") < 0;
    		}).sort((a,b) => {

    			if(a.attr === attributes && b.attr !== attributes) return -1;
    			if(b.attr === attributes && a.attr !== attributes) return +1;

    			if(a.rarity != b.rarity) return (b.rarity || 0) - (a.rarity || 0)

    			var ar = (a.skill.scoreUpRate || 0), br = (b.skill.scoreUpRate || 0);
    			if(ar != br) return br - ar;

    			var e = opts.event;
    			var isChallengeBonus = !opts.is_free && ["challenge", "versus"].includes(e.eventType);
    			var pb = e.detail.paramBonus[0];
    			var upKeyName = Object.keys(pb).sort((a,b) => {
    				if(typeof pb[a] === "string") { return +1; }
    				if(typeof pb[b] === "string") { return -1; }
    				var ar = pb[a], br = pb[b];
    				return br - ar;
    			})[0];
    			var au = (a.simpleParams.max[upKeyName] || 0),
    				bu = (b.simpleParams.max[upKeyName] || 0);
    			return bu - au ;
    		})[0];

    		obs.trigger(`cardDataSetTo_card0${i+1}`, pickCard, cards.indexOf(pickCard))
    	})

    }.bind(this)

    this.onChangeTotalValue = function(e) {
    	this.bandTotalValue = Math.max($("#bandTotalVal").val(), 0);
    }.bind(this)
});
}).call(this,typeof global !== "undefined" ? global : typeof self !== "undefined" ? self : typeof window !== "undefined" ? window : {})
