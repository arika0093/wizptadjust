    var $ = require("jquery");

    const starPrices = [
    	{ name: "A", num:   60, price:  120 },
    	{ name: "B", num:  360, price:  480 },
    	{ name: "C", num:  760, price:  960 },
    	{ name: "D", num: 1300, price: 1600 },
    	{ name: "E", num: 2600, price: 3200 },
    	{ name: "F", num: 4200, price: 5000 },
    	{ name: "G", num: 8400, price: 9800 },
    	{ name: "C(SP)", num: 1200, price:  960, is_used: false },
    	{ name: "E(SP)", num: 4000, price: 3200, is_used: false },
    ];

    document.title = "BangDream Statistics";

    var p = opts.query.params;
    $(() => {
    	if(p.length > 0){
    		setTimeout(() => {
    			$(`#${p[0]}`)[0].scrollIntoView(true);
    		}, 1000)
    	}
    })

    this.on("mount", () => {
    	$(".calcStar input").on("change keyup", () => {
    		var num = $("#calcStarNum").val() - 0;
    		var isc = $("#calcStarIsCampaign").prop("checked");
    		var price = 0;

    		starPrices.forEach((e,i) => {
    			e.is_used = false,
    			e.index = i,
    			e.effort = e.price/e.num;
    		});
    		while(num > 0){
    			var itemOfBestEffort = starPrices.filter(e => {
    				return true
    			}).filter(e => {
    				return !e.name.includes("SP") || (isc && !e.is_used)
    			}).sort((a,b) => {
    				var N = -760

    				if(num - a.num <= N && num - b.num >  N) return +1;
    				if(num - a.num >  N && num - b.num <= N) return -1;

    				if(num - a.num <= 0 && num - b.num <= 0) return a.num - b.num;

    				return a.effort - b.effort;
    			})[0];
    			num -= itemOfBestEffort.num;
    			price += itemOfBestEffort.price;
    			starPrices[itemOfBestEffort.index].is_used = true;
    		}
    		$("#calcStarPrice").text(`必要な額は ${price} 円`)
    		return true;
    	});
    })
});
