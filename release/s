    this.onClickCard = (() => {
    	var c = opts.card;
    	if(opts.ronly){

    		return;
    	} else if(opts.chkmode){
    		c.ischk = !c.ischk;

    		storageJson.saveArrays('cardlist.have', c.cardId, c.ischk);
    	} else if(!c.isnf) {
    		if(imageOpened != true) {

    			imageOpened = true;
    			this.viewImageUrl = imageUrlGen(c, opts.isafter);

    		} else {
    			imageOpened = false;
    		}
    	}
    	try {
    		this.update();
    	} catch(e) {

    	}
    }).bind(this);

    this.viewImageOnClosed = (e => {
    	this.viewImageUrl = null;
    	this.update();
    }).bind(this);

    function imageUrlGen(card, isAfterTraining = false){
    	var par = card.imageUrlParentDir || `${global.RESROOT}/assets-jp`;
    	var CardImageURL = `${par}/characters/resourceset/{0}_rip/` +
    		`card_${isAfterTraining && card.rarity >= 3 ? "after_training" : "normal"}.png`;
    	return stringFormat(CardImageURL, card.cardRes);
    }
});
