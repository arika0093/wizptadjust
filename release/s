    const CardsAPI = `${global.APIROOT}/card?rarity[]=4&rarity[]=3`;
    const SongsAPI = `${global.APIROOT}/music`;
    const EventAPI = `${global.APIROOT}/event`;
    const ChartAPI = `${global.APIROOT}/music/chart/{0}/{1}`;
    const SkillAPI = `${global.APIROOT}/skill`;

    var $ = require("jquery");
    var bndrScoreCalc = require("../../js/scorecalc/bndrScoreCalc.js");
    var stringFormat = require("../../js/util/stringFormat.js");
    var accessApi = require("../../js/bndrUtil/getJsonFromMultiServer.js");
    var obs = global.Observer;

    document.title = "BangDream Score Calculator";

    this.on("mount", async () => {
    	this.songsList = await accessApi.getSongs();
    	this.cardsList = await accessApi.getCards([4,3]);
    	this.skillsList = await accessApi.getSkillsList();
    	this.currentEvent = await accessApi.getCurrentEvent();

    	this.skillsList.forEach(e => {
    		var sr = e.simpleDescription.match(/([0-9]{2,})[%％][UＵ][PＰ]/);
    		e.scoreUpRate = sr ? sr[1] - 0 : 0;
    	});

    	this.cardsList.data.forEach(e => {
    		e.skill = $.extend(true, e.skill, this.skillsList.find(s => {
    			return s.skillId === e.skill.skillId;
    		}));
    	});

    	var evSongs = this.currentEvent.detail.musics;
    	if(evSongs){
    		var isContained = (s) => evSongs.map(e => e.musicId).indexOf(s.musicId) >= 0;
    		this.songsList.data.sort((a,b) => {
    			if( isContained(a) && !isContained(b)) return -1;
    			if(!isContained(a) &&  isContained(b)) return +1;
    		});
    	}

    	this.update();

    	obs.trigger("chosenUpdateSongs");
    	obs.trigger("chosenUpdateCards");
    });

    obs.on("calculationStart", async (obj) => {

    	var ch = await accessApi.getChart(obj.songData.musicId, obj.songDifficult);
    	this.chartId = obj.songData.musicId;
    	this.chartDataOrigin = $.extend(true, {}, ch);
    	ch.objects = ch.objects.filter(e => e.type !== "System");
    	obj.songChartData = this.chartData = ch;

    	this.calcObject = obj;
    	this.overlayChartData = null;
    	this.calculating = true;
    	this.update();

    	setTimeout(() => {
    		bndrScoreCalc.scoreCalculate(obj).then(result => {
    			this.resultData = result;
    			this.overlayChartData = result.bestTimingNoteDetailAll;
    			obs.trigger("calculationEnd", result, obj);
    		}).catch(e => {
    			console.error(e);
    		}).finally(() => {

    			this.calculating = false;
    			this.update();
    		});
    	}, 0);
    });

    obs.on("calculate_info", async (msgs, infos) => {

    })

    obs.on("calculationEnd", async (result, obj) => {
    	console.log(result);
    })
});
}).call(this,typeof global !== "undefined" ? global : typeof self !== "undefined" ? self : typeof window !== "undefined" ? window : {})
