    var $ = require("jquery");
    var imgloaded = require("imagesloaded");
    var storageJson = require("../../js/util/storageJson.js");
    var limitedList = require("../../js/bndrUtil/limitedTimestamps.js");
    var accessAPIs = require("../../js/bndrUtil/getJsonFromMultiServer.js");
    var zlib = require("zlib");

    document.title = "BangDream CardViewer";

    var q = opts.query;
    this.rstHash = q.hash || null;
    global.storageReadOnly = this.isReadOnly = !!this.rstHash;
    this.chkmode = this.isReadOnly;
    this.imageSize = 80;
    this.isLoadEnd = false;
    this.update();

    var t = null;
    var updateImageSize = () => {
    	var width = $("body").width();

    	this.imageSize = Math.max(
    		Math.round( width / 16 / 5 ) * 5,
    		60
    	);

    	this.update();
    }
    $(window).on("load resize", () => {
    	clearTimeout(t);
    	t = setTimeout(() => {
    		updateImageSize();
    	}, 150);
    })
    updateImageSize();

    this.on("mount", async() => {

    	this.cardsListOrigin = await accessAPIs.getCards([1,2,3,4]);
    	this.skillsList = await accessAPIs.getSkillsList();

    	this.skillsList.forEach(e => {

    		skillAdditionalInfo(e);
    	});

    	this.cardsListOrigin.data.forEach(e => {
    		var skill = this.skillsList.find(s => {
    			return s.skillId === e.skill.skillId;
    		});
    		e.udt = {
    			skillRate: skill.scoreUpRate,
    			skillDesc: skill.ssimpleDesc,
    		}
    	});
    	this.cardsList = $.extend(true, {}, this.cardsListOrigin);
    	this.onChangeOption();

    	setTimeout(() => {
    		this.isLoadEnd = true;
    		this.update();
    	}, 1000);
    });

    this.cvf = {};
    this.onChangeOption = ((e, c, s) => {

    	if(e && s){
    		if(s.includes("isAllShow")){

    			var checks = $('cv-option-scores checkutil[type!="allshow"]');
    			checks.each((i,e) => {
    				var t = $(e);
    				var a = t.attr("val").split(".").slice(-1)[0];
    				t.find('input[type="checkbox"]').prop("checked", c);
    				this.cvf[a] = c;
    			})
    		} else {

    			var checks = $('cv-option-scores checkutil[type!="allshow"] input[type="checkbox"]');
    			var asc = $('cv-option-scores checkutil[type="allshow"] input[type="checkbox"]');
    			var f = true;
    			checks.each((i,e) => {
    				f = f && $(e).prop("checked");
    			})
    			asc.prop("checked", f);
    		}
    	}

    	var cardlistHaveIds = [];
    	if(!this.rstHash) {
    		cardlistHaveIds = storageJson.load("cardlist.have") || [];
    	} else {
    		var s = zlib.inflateSync(new Buffer(decodeURIComponent(this.rstHash), "base64"));
    		cardlistHaveIds = JSON.parse(s);
    	}

    	this.cardsList.data = $.extend(true, [], this.cardsListOrigin).data.filter(e => {
    		var cvf = this.cvf;
    		var isLimited = (c) => {
    			return (c.distType == "limited") ||  (limitedList.includes(c.releasedAt) && e.gachaText);
    		};
    		var isDist = (c) => {
    			return (["event", "camaign"].includes(c.distType)) || !e.gachaText;
    		};
    		var s = this.skillsList.find(s => {
    			return s.skillId === e.skill.skillId;
    		});
    		var i = s.sinfo;

    		var showType = (false
    			|| (cvf.is_score && (!i.is_lifeup && !i.is_lifeor && !i.is_judge))
    			|| (cvf.is_lifeheal && (i.is_lifeup || i.is_lifeor))
    			|| (cvf.is_judge && i.is_judge)
    		);
    		var showRarity = (false
    			|| (cvf.rare4 && e.rarity == 4)
    			|| (cvf.rare3 && e.rarity == 3)
    			|| (cvf.rare2 && e.rarity == 2)
    			|| (cvf.rare1 && e.rarity == 1)
    		);
    		var showDist = (false
    			|| (cvf.is_limited && isLimited(e))
    			|| (cvf.is_unlimited && !isLimited(e))
    			|| (cvf.is_dist && !isDist(e))
    		);
    		return showType && showRarity;

    	}).map(e => {
    		var r = e.udt.skillRate;
    		var cvf = this.cvf;

    		var isVisible = ( false
    			|| (cvf.is_m30p && r <= 30)
    			|| (cvf.is_40p && r == 40)
    			|| (cvf.is_60p && r == 60)
    			|| (cvf.is_65p && r == 65)
    			|| (cvf.is_80p && r == 80)
    			|| (cvf.is_100p && r == 100)
    			|| (cvf.is_105p && r == 105)
    			|| (cvf.is_110p && r == 110)
    			|| (cvf.is_115p && r == 115)
    			|| (cvf.is_120p && r == 120)
    			|| (cvf.is_125p && r == 125)
    		);
    		e.isnf = !isVisible;

    		e.ischk = cardlistHaveIds.includes(e.cardId);
    		e.islimited = limitedList.includes(e.releasedAt);
    		e.isdist = !e.gachaText && e.rarity >= 3;

    		return e;
    	});

    	this.update();
    }).bind(this);

    this.jumpToAttr = ((e) => {
    	var t = $(e.target);
    	var j = t.attr("jumpto");
    	var to = $(`cv-cardbands.${j}`);
    	to[0].scrollIntoView(true);
    }).bind(this);

    this.aftrainingUpdate = ((e) => {
    	this.isLoading = true;
    	imgloaded($(`img`), () => {
    		this.isLoading = false;
    		this.update();
    	});
    	this.update();
    }).bind(this);

    this.shareUrlGenerate = (() => {
    	var chs = storageJson.load("cardlist.have");
    	zlib.deflate(JSON.stringify(chs), (err, gzb) => {
    		var shareUrlHash = gzb.toString("base64");
    		this.shareUrl = `${location.origin}/#!/cardviewer/hash:${encodeURIComponent(shareUrlHash)}`;
    		alert(`URLを生成しました！アドレスバーに反映されるのでコピーして利用してください。`);
    		history.replaceState("","",this.shareUrl);
    	});
    }).bind(this);

    function skillAdditionalInfo(e){
    	var sd = e.simpleDescription;
    	var d = e.description;
    	var sr = sd.match(/([0-9]{2,})[%％][UＵ][PＰ]/);
    	e.sinfo = {};
    	e.sinfo.is_judge  = e.skillSortType.includes("judge");
    	e.sinfo.is_lifeup = e.skillSortType.includes("heal_with");
    	e.sinfo.is_lifeor = e.skillSortType.includes("heal_or");
    	var tmatch = e.sinfo.is_tmatch = d.match(/発動者のバンド編成が.*タイプのみの/);
    	var bmatch = e.sinfo.is_bmatch = !tmatch && d.match(/発動者のバンド編成が/);
    	e.sinfo.is_pfonly = !bmatch && sd.match(/PERFECTのみ/);
    	e.sinfo.is_ugreat = !tmatch && sd.match(/GREAT以下を出すまで/);

    	var r = e.scoreUpRate = sr ? sr[1] - 0 : 0;
    	var d = "", i = e.sinfo;
    	if(i.is_lifeor){
    		d = `${r}/♥`;
    	} else {
    		d = `
    			${i.is_lifeup ? "♥" : ""}
    			${i.is_judge  ? "●" : ""}
    			${r}
    			${i.is_pfonly ? "P" : ""}
    			${i.is_ugreat ? "G" : ""}
    			${i.is_tmatch ? "T" : ""}
    			${i.is_bmatch ? "B" : ""}
    			`;
    		d = d.replace(/\s/g, "");
    	}
    	e.ssimpleDesc = d;
    }
});
