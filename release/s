    var chartManager = require("../../js/bndrUtil/chartManage.js");
    var findBandOrder = require("../../js/bndrUtil/bandOrder.js");
    var accessApi = require("../../js/bndrUtil/getJsonFromMultiServer.js");
    var $ = require("jquery");
    var numeral = require("numeral");

    this.loading = true;
    var qs = opts.query;
    var id = qs["id"] || null;
    var df = qs["diff"] || null;
    if(id && df){
    	var path = `${id}-${df}`
    	var cs = chartManager(path).then(cs => {
    		if(!cs || !cs[path]){
    			this.err = `Chart load failed. [ID: ${id}, Difficult: ${df}]`;
    		} else {
    			this.cdata = cs[path];
    			this.chart = cs[path].chart;

    			var s = this.cdata.song;
    			this.songid = id;
    			this.jacketUrl = `${s.imageUrlParentDir}${s.jacket}`;
    			this.songTitle = s.title;
    			document.title = `${this.songTitle} - BangDream ChartViewer`;
    		}
    		this.update({
    			loading: false
    		});
    	});
    } else {

    	songListUpdate(this);
    }

    async function songListUpdate (this_, options = {}) {
    	var {filter, order, asc} = options;
    	var songs = await accessApi.getSongs();
    	this_.musicsList = songs.data.filter(e => {
    		return !filter || e.title.includes(filter);
    	}).sort((a,b) => {
    		var rst = 0;
    		var lvDf = Math.max(...b.difficulty) - Math.max(...a.difficulty);
    		var exDf = b.difficulty[1] - a.difficulty[1];
    		var bdDf = findBandOrder(a.bandId) - findBandOrder(b.bandId);
    		var pbDf = a.musicId - b.musicId;
    		var cvDf = (a.tag.includes("anime")) - (b.tag.includes("anime"));
    		switch(order){
    			case 3:

    				rst = pbDf * -1;
    				break;
    			case 1:

    				rst = lvDf || exDf;
    				break;
    			case 2:
    			default:

    				rst = exDf;
    				break;
    		}
    		var toggleRst = rst * ((asc || -1) * -1)
    		return toggleRst || cvDf || bdDf || pbDf;

    	}).map(e => {
    		var [easy, expert, hard, normal, special] = e.difficulty;
    		return {
    			title: e.title,
    			id: e.musicId,
    			urlb: `/#!/chartview/id:${e.musicId}/diff:`,
    			jacket: `${e.imageUrlParentDir}${e.jacket}`,
    			easy,
    			expert,
    			hard,
    			normal,
    			special,
    			maxLevel: Math.max(...e.difficulty),
    			isCover: e.tag.includes("anime"),
    			isSpContained: special >= 0,
    		}
    	});
    	document.title = `BangDream ChartViewer`;
    	this_.update({
    		loading: false
    	});
    }

    this.filterUpdated = () => {
    	var t = $("#searchFilter").val();
    	var ord = $(`input[name="order"]:checked`).val() - 0;
    	var asc = $(`input[name="ascdesc"]:checked`).val() - 0;

    	songListUpdate(this, {
    		filter: t,
    		order: ord,
    		asc,
    	});
    }

    this.formatNum = (n, f) => {
    	return numeral(n).format(f);
    }
});
