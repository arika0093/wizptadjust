    	return getCharaName(id);
    }.bind(this)

    function setCardDetailDatas(selcard){
    	const isAfterTraining = true;
    	const bandLists = getBandCName.Bands;

    	this.cardImageUrl = getCharaImg(selcard, isAfterTraining);

    	selcard.characterName = getCharaName(selcard.characterId);

    	var bandId = getBandCName.charaId2BandId(selcard.characterId);
    	this.cardAttrAndBand =
    		`[ ${bandLists[bandId]} ]` +
    		`[ ${capitalizeFirstLetter(selcard.attr)} ]`;

    	(setSkillDescription.bind(this))(selcard);

    	triggerSendToCardsView(selcard, this.skillLevel);
    }

    function setSkillDescription(selcard){
    	var skillLv = this.skillLevel = this.skillLevel || 5;
    	var skillDetail = selcard.skill;
    	var desc = skillDetail.simpleDescription;
    	this.cardSkillDescription = desc;
    	selcard.skillSimpleDescription = desc;

    }

    function triggerSendToCardsView(selcard, skillLv){
    	obs.trigger(`cardDataSetFromCardView`, selcard, skillLv, opts.ref);
    }

    function capitalizeFirstLetter(string){
    	return string.charAt(0).toUpperCase() + string.slice(1);
    }
});
