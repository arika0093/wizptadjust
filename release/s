		});
		*/
	},
	
	// init
	scoreCalculate: async function (obj) {
		var result = {};
		// --------------------
		// 前準備として譜面データをソートしておく
		obj.songChartData.objects = this.sortChartData(obj.songChartData.objects);
		
		// --------------------
		// スキル順が指定されてるならそれを使う
		// 指定されてないなら全パターンを試し、一番よさげなものを使う
		var allJustBestSkillOrder = {};
		this.debugMsg(obj, {0: "all-just score calculate" });
		if(!obj.setting.skillManual){
			this.debugMsg(obj, {1: "calc all pattern as brute force" });
			var calculatedScores = await this.findMostScoreOrder(obj);
			allJustBestSkillOrder = calculatedScores[0];
		} else {
			this.debugMsg(obj, {1: "use specified order" });
			var skillOrder = obj.setting.skillManual.split(",");
			allJustBestSkillOrder = this.generateAJOrderObject(obj, skillOrder);
		}
		// 全ジャスト時の最高スコアと並び順
		var allJustTotalScore = allJustBestSkillOrder.scoreOnAllJust;
		this.debugMsg(obj, {
			1: "all-just score calc finished",
			2: ("pattern: " + (allJustBestSkillOrder.patternToString) + ", score: " + allJustTotalScore)
		});
		
		var bestTimingScores = {}, bestTimingScoresOfAllPattern = [];
		if(!obj.setting.isNotCalcBestScore){
			this.debugMsg(obj, {0: "best score calculate" });
			if(obj.setting.allCalculate) {
				// 全パターン捜索
				this.debugMsg(obj, {1: "simulate note shifted as brute force (CAUTION: too long!)" });
				bestTimingScoresOfAllPattern = await this.getBestTimingScoresOfAllPattern(calculatedScores, calculatedScores.length, obj);
				bestTimingScores = bestTimingScoresOfAllPattern[0];
			} else if(!obj.setting.skillManual){
				// 上位数パターン探索
				var c = obj.setting.trySkillOrders;
				this.debugMsg(obj, {1: ("simulate note shifted as some likely pattern (N=" + c + ")") });
				bestTimingScoresOfAllPattern = await this.getBestTimingScoresOfAllPattern(calculatedScores, c, obj);
				bestTimingScores = bestTimingScoresOfAllPattern[0];
			} else {
				// AJ最高スコアを出せる組み合わせにおいてBestScoreの計算
				this.debugMsg(obj, {1: "use specified order" });
				bestTimingScores = this.calculateBestTimingScores(allJustBestSkillOrder, obj);
			}
			var allShiftedNotes = bestTimingScores.allShiftedNotes;
			var bestTimingList = bestTimingScores.bestTimingList;
			var bestScorePattern = bestTimingScores.bestScorePattern;
			var bestTimingNoteDetailAll = bestTimingScores.bestTimingNoteDetailAll;
			var bestTimingNoteDetailExamples = bestTimingScores.bestTimingNoteDetailExamples;
			var bestTotalScore = bestTimingScores.bestTotalScore;
			
			this.debugMsg(obj, {
				1: "best score calc finished",
				2: ("pattern: " + (bestScorePattern.patternToString) + ", score: " + bestTotalScore),
			});
		}
		
		
		var calcResult = {
			// 全スキルパターンの一覧
			calculatedScores: calculatedScores,
			// 全部JUSTで取ったときのスキル込スコアとその関連データ
			allJustBestSkillOrder: allJustBestSkillOrder,
			// ずらしの全パターン
			allShiftedNotes: allShiftedNotes,
			// 最良タイミング一覧(各スキルごと)
			bestTimingList: bestTimingList,
			// 最良パターン捜索結果
			bestTimingScoresOfAllPattern: bestTimingScoresOfAllPattern,
			// 最良タイミングの一例(全部合算)
			bestTimingNoteDetailAll: bestTimingNoteDetailAll,
			bestTimingNoteDetailExamples: bestTimingNoteDetailExamples,
			// 最高スコア(予想)
			bestTotalScore: bestTotalScore,
			// 全JUST時最高スコア(予想)
			allJustTotalScore: allJustTotalScore,
			// 最高スコアを出すためのスキル順
			bestScorePattern: bestScorePattern,
		};
		
		// Abstractを錬成
		if(!obj.setting.notNeedAbstract){
			calcResult.resultAbstract  = this.generateResultAbstract(obj, calcResult);
			calcResult.newClass4Csharp = this.generateReuseCode(obj, {
				totalScore: bestTotalScore,
				notesDetail: bestTimingNoteDetailAll,
				pattern: bestScorePattern.pattern,
			});
		}
		
		return calcResult;
	},
	
	// 理想スキル順を探す
	findMostScoreOrder: async function(obj) {
		var this$1 = this;

		// 全組み合わせ取得
		var allSkillOrders = this.listupAllSkillOrders();
		// 各パターンについてtoStringをかけ、重複を消す
		var onlySkillOrders = allSkillOrders.map(function (e,i) {
			return {
				pattern: e,
				patternToString: this$1.getPatternToString(e, obj.setCards),
				index: i,
			}
		}).filter(function (x,i,self) {
			return self.map(function (e) { return e.patternToString; }).indexOf(x.patternToString) === i;
		});
		
		var onlyCount = onlySkillOrders.length;
		var allSkillScores = onlySkillOrders.map(function (e, i) {
			var s = this$1.calcScoreOnAllJustTiming(obj, e.pattern);
			e.scoreOnAllJust = s.simpleScore;
			e.scoreDetail = s.scoreOfNotes;
			e.notesDetail = s.stateOfNotes;
			e.containSkillNotes = s.skillContainNotes;
			
			if(((i+1)*100/onlyCount) % 20 === 0){
				this$1.debugMsg(obj, {2: ("calculate finished: " + ((i+1)*100/onlyCount) + "% ( " + (i+1) + "/" + onlyCount + " )") });
			}
			
			return e;
		});
		allSkillScores.sort(function (a,b) {
			// スコア順にソート
			return b.scoreOnAllJust - a.scoreOnAllJust;
		});
		
		return allSkillScores;
	},
	
	// スキル発動順の全パターンを取得
	listupAllSkillOrders: function() {
		var Units = [1, 2, 3, 4, 5];
		// 最初の5スキルの全発動順(120通り)
		var perm5 = jc.permutation(Units).toArray();
		// と、最後のスキル(5通り)
		var perm6 = [];
		perm5.forEach(function (e) {
			Units.forEach(function (u) {
				perm6.push(e.concat(u));
			})
		});
		return perm6;
	},
	
	// BestScoreの各パターンを網羅する
	getBestTimingScoresOfAllPattern: async function(calculatedScores, countNum, obj){
		var this$1 = this;

		var s_cs = calculatedScores.slice(0, countNum);
		var csa = s_cs.map(function (e) {
			this$1.debugMsg(obj, {2: ("skill order: " + (e.patternToString)) });
			return this$1.calculateBestTimingScores(e, obj);
		});
		csa.sort(function (a, b) {
			return b.bestTotalScore - a.bestTotalScore;
		});
		return csa;
	},
	
	// 並び順を渡されたら、各種データを詰めて返す
	generateAJOrderObject: function(obj, skillOrder){
		var s = this.calcScoreOnAllJustTiming(obj, skillOrder);
		return {
			pattern: skillOrder,
			patternToString: this.getPatternToString(skillOrder, obj.setCards),
			scoreOnAllJust: s.simpleScore,
			scoreDetail: s.scoreOfNotes,
			notesDetail: s.stateOfNotes,
			containSkillNotes: s.skillContainNotes,
		};
	},
	
	// 単純に全ノーツをALL JUSTで捌いた場合のスコアを計算する
	calcScoreOnAllJustTiming: function(obj, pattern){
		var NoSkill = { uprate: 0, startTiming: 0, endTiming: undefined };
		var simpleScore = 0;
		var chartObj = obj.songChartData.objects;
		var skillData = clone(NoSkill);
		var skillIndex = 0;
		var skillContainNotes = [];
		var stateOfNotes = chartObj
			.map(function (e, index) {
				var timing = e.timing;
				var effect = e.effect, property = e.property;
				var lane = e.lane;
				var scoreUpEnd = skillData.endTiming;
				// 時間が経過してたらスキル無効化
				if(scoreUpEnd > 0 && scoreUpEnd < timing) {
					skillData = clone(NoSkill);
					scoreUpEnd = undefined;
				}

				// ノーツあたりのスコア計算
				var judge = "perfect";
				var cmb = index + 1;
				var scoreUpRate = skillData.uprate;
				if(scoreUpRate > 0){
					skillContainNotes[skillIndex - 1] = (skillContainNotes[skillIndex - 1] || 0) + 1;
				}
				
				// スキル有効化
				// スキルノーツ自体は含まれない
				if(effect === "Skill"){
					var idx = pattern[skillIndex] - 1;
					var s = obj.setCards[idx].skillData;
					skillData.startTiming = e.timing;
					skillData.endTiming = e.timing + s.scoreDuration;
					skillData.uprate = s.scoreUpRate;
					skillIndex++;
				}
				return {
					judge: judge, cmb: cmb, index: index, lane: lane, effect: effect, property: property, scoreUpRate: scoreUpRate, timing: timing, scoreUpEnd: scoreUpEnd
				};
			});
		var scoreOfNotes = this.getScoreOfNotes(stateOfNotes, obj);
		simpleScore = arraySum(scoreOfNotes);
		return {
			simpleScore: simpleScore, scoreOfNotes: scoreOfNotes, stateOfNotes: stateOfNotes, skillContainNotes: skillContainNotes
		};
	},
	
	// 指定されたスキル順において、全組み合わせの中から最高スコアをpickupする
	calculateBestTimingScores: function(allJustBestSkillOrder, obj){
		var this$1 = this;

		// --------------------
		// Bestスキル順を使用し、全スキルノーツ前後におけるずらしをSimulate
		var ajbso = allJustBestSkillOrder;
		var allShiftedNotes = ajbso.pattern.map(function (e, i) {
			this$1.debugMsg(obj, {3: ("note shift at S" + (i+1) + " (unit " + e + ")") });
			return this$1.listupAllShiftedNotesPatterns(obj, ajbso, i);
		});
		
		// --------------------
		// 各ずらしリストにおいて、実際にスコア計算を行い一番いいやつをpickup
		this.debugMsg(obj, {3: "extract max score pattern in each shifted list" });
		var notesDetailOfBestTiming = clone(ajbso.notesDetail);
		var bestTimingList = allShiftedNotes.map(function (e, i) {
			// 最もスコアが高くなるパターンを抽出
			this$1.debugMsg(obj, {4: ("find better pattern in S" + (i+1)) });
			var timingShiftedScores = e.map(function (en) {
				var nd = clone(notesDetailOfBestTiming);
				// contain notes of skill
				var containNotesOfSkill = en.notesDetail.filter(function (e) {
					return e.effect !== "Skill";
				});
				var containNotesCountOfSkill = containNotesOfSkill.length;
				var containGreatNotesCount = en.notesDetail.filter(function (e) {
					return e.judge !== "perfect";
				}).length;
				
				// conbine chart data
				nd = this$1.conbineChartData(nd, en.notesDetail);
				// score calculate
				var scores = this$1.getScoreOfNotes(nd, obj);
				var scoreSum = arraySum(scores);
				return {
					scoreSum: scoreSum,
					scores: scores,
					shiftedData: en,
					detail: nd,
					detailOfPinpoint: en.notesDetail,
					shiftFrameSum: en.shiftFrameSum,
					shiftCoefficient: en.shiftCoefficient,
					containNotesCountOfSkill: containNotesCountOfSkill,
					containGreatNotesCount: containGreatNotesCount
				}
			}).sort(function (a, b) {
				if(a.scoreSum !== b.scoreSum){
					return (b.scoreSum - a.scoreSum);
				}
				// スコアが同じ場合は、ずらし優先度に則って決める
				var prim = obj.setting.diffNotesPrimary;
				switch(prim){
					case "FAST_PRM":
						return (b.shiftFrameSum - a.shiftFrameSum);
					case "SLOW_PRM":
						return (a.shiftFrameSum - b.shiftFrameSum);
					case "MIN_DIFF":
					default:
						return (a.shiftCoefficient - b.shiftCoefficient);
				}
			});
			
			// 最良パターンをまとめる
			var t = timingShiftedScores[0];
			var bestScore = t.scoreSum;
			var bestPatterns = timingShiftedScores.filter(function (e) { return e.scoreSum === bestScore; });
			var bestPatternNoteDetailExample = t.detailOfPinpoint;
			var containAllNotesCount = t.containNotesCountOfSkill;
			var containGreatNotesCount = t.containGreatNotesCount;
			
			return {
				bestScore: bestScore,
				bestPatterns: bestPatterns,
				bestPatternsCount: bestPatterns.length,
				bestPatternNoteDetailExample: bestPatternNoteDetailExample,
				containAllNotesCount: containAllNotesCount,
				containGreatNotesCount: containGreatNotesCount
			}
		});
		
		// --------------------
		// 得られた各地点でのBest Timingを元に最終スコアを算出
		this.debugMsg(obj, {3: "calc final-score by combine each shifted notes data" });
		var bestTimingNoteDetailExamples = bestTimingList.reduce(function (a, x) {
			a.push.apply(a, (x.bestPatternNoteDetailExample));
			return a;
		}, []);
		var bestScores = [], bestTotalScore = 0, bestTimingNoteDetailAll = null;
		{
			var ndbt = clone(notesDetailOfBestTiming);
			bestTimingNoteDetailAll = this.conbineChartData(ndbt, bestTimingNoteDetailExamples);
			bestScores = this.getScoreOfNotes(bestTimingNoteDetailAll, obj);
			bestTotalScore = arraySum(bestScores);
		}
		return {
			allShiftedNotes: allShiftedNotes,
			bestTimingList: bestTimingList,
			bestTimingNoteDetailAll: bestTimingNoteDetailAll,
			bestTimingNoteDetailExamples: bestTimingNoteDetailExamples,
			bestTotalScore: bestTotalScore,
			bestScorePattern: {
				pattern: ajbso.pattern,
				patternToString: ajbso.patternToString,
			},
		};
	},
	
	// 指定されたスキル順において、ずらしパターンを列挙する
	listupAllShiftedNotesPatterns: function(obj, ajbso, index){
		var this$1 = this;

		var SecondOf1Frame = this.convertFrameToSecond(1, obj);
		var SecondOfSkillActivate = (function (st) {
			var sl = st.sameLineSkillActive, dot5 = st.dot5frameEnabled;
			if(sl)      { return 0; }
			if(dot5)    { return SecondOf1Frame / 2; }
			else        { return SecondOf1Frame; }
		})(obj.setting);
		
		var stateOfNotes = clone(ajbso.notesDetail);
		// 発動するスキルの情報
		var activeSkillIndex = ajbso.pattern[index] - 1;
		var activeSkillData = obj.setCards[activeSkillIndex].skillData;
		var activeSkillDuration = activeSkillData.scoreDuration
			+ this.convertFrameToSecond((obj.setting.frameST || 0), obj);
		// 上昇率は場合によって異なる可能性があるので外部に投げる
		var activeSkillUpRate = this.createSkillRateFromSkillData(obj, activeSkillData);
		
		// スキル範囲前のノーツを巻き込める猶予F数
		// パフェ限スキル(115P/110G)の場合はGRで巻き込まない
		var isPerfectSkill = activeSkillData.isPerfectOnly || activeSkillData.isContinuedPerfectOnly;
		
		// スキル範囲後のノーツを巻き込む猶予F数
		// パフェ限スキル(115P)の場合はGRで巻き込まない
		var isPerfectOnly = activeSkillData.isPerfectOnly && !activeSkillData.isContinuedPerfectOnly;
		
		// 対象がLNだった場合、判定幅を広げる
		var targetSkillNote = obj.songChartData.objects.filter(function (e) { return e.effect === "Skill"; })[index];
		var usingGrace = this.calcLongNoteJudgeFrames(targetSkillNote, obj);
		var ug_b = usingGrace.before, ug_a = usingGrace.after;
		
		// 同時押しOption用
		var isContainSkillFirst = function (t_a, t_b) {
			var sl = obj.setting.sameLineSkillActive;
			if(sl) { return t_a >= t_b }
			else { return t_a > t_b }
		};
		
		// generate range from ug_b to ug_a
		var allFrameOfSkillNote = [];
		var d5e = obj.setting.dot5frameEnabled;
		var add_f = d5e ? 0.5 : 1.0;
		var startF = d5e ? ug_b : Math.floor(ug_b);
		for(var i = -startF; i <= ug_a; i += add_f){
			allFrameOfSkillNote.push(i);
		}
		
		// 全スキルノーツFについて総当たり捜査
		var allPatternOfSkillNoteFrame = allFrameOfSkillNote.map(function (e) {
			// 各処理ごとに譜面データをコピー(timingを上書きすることで処理するため)
			var chartObj = clone(obj.songChartData.objects);
			// 厳格に処理するときはtimingをF更新時にずらす
			if(obj.setting.enableStrictFrames){
				chartObj.forEach(function (e) {
					e.timing = this$1.shiftNoteTimingWithFrame(e.timing, obj, "ceil");
				});
			}
			
			// 該当スキル箇所を抽出
			var targetSkillNote = chartObj.filter(function (e) { return e.effect === "Skill"; })[index];
			var targetSkillNoteIndex = chartObj.indexOf(targetSkillNote);
			// ずらす秒数を計算しておく
			var shiftSecond = this$1.convertFrameToSecond(e, obj);
			var timingWithJustFrame = targetSkillNote.timing;
			var shiftedSkillNoteTiming = timingWithJustFrame + shiftSecond;
			// スキルノーツをずらす
			targetSkillNote.defaultTiming = targetSkillNote.timing;
			targetSkillNote.timing = shiftedSkillNoteTiming;
			// スキル範囲ノーツを取得
			var notesOfSkillArea = chartObj.filter(function (e) {
				var skillEnd = shiftedSkillNoteTiming + activeSkillDuration;
				var skillEndFixed = this$1.deleteMinFraction(skillEnd);
				return isContainSkillFirst(e.timing, shiftedSkillNoteTiming)
					&& e.timing <= skillEndFixed;
			});
			// スキル範囲手前ノーツを巻き込み
			var lanesListOfBefore = [targetSkillNote.lane];
			var notesOfSkillBefore = chartObj.filter(function (e, i) {
				var graceFrameWillStartSkill = this$1.calcLongNoteJudgeFrames(e, obj, { isPerfect: isPerfectSkill });
				// スキルノーツ手前のノーツを巻き込むので「after」(該当ノーツの後半判定を参照する)
				var graceS = graceFrameWillStartSkill.after;

				// スキルに含められるか無理やり探す
				// TODO: 2行目の>は同時押しでねじ込めるなら要修正(SecondOfSkillActivateを使用することでfix)
				var secOfBeforeSkill = this$1.convertFrameToSecond(graceS, obj);
				var adf = SecondOfSkillActivate;
				var minContain = shiftedSkillNoteTiming - secOfBeforeSkill + adf;
				var minContFix = this$1.deleteMinFraction(minContain);
				var isContain = e.timing <= shiftedSkillNoteTiming
					&& e.timing >= minContFix && i !== targetSkillNoteIndex
				
				// 同一レーンのノーツは巻き込みようがないので外す
				if(!isContain || lanesListOfBefore.includes(e.lane)){
					return false;
				} else {
					lanesListOfBefore.push(e.lane);
					return true;
				}
			}).map(function (e) {
				// スキルノーツから1(0.5)フレーム後に叩く
				// TODO: 同時押しでもOKならここは要修正(Option化した)
				e.defaultTiming = e.timing;
				e.timing = shiftedSkillNoteTiming + SecondOfSkillActivate;
				return e;
			});
			var notesOfSkillAfter = chartObj.filter(function (e) {
				// TODO: 判定強化が入っている場合、スキル後にそれに準じて猶予が増える
				var graceFrameFinishedSkill = this$1.calcLongNoteJudgeFrames(e, obj, {
					isPerfect: isPerfectOnly,
					judgeSkillTarget: activeSkillData.judgeTargetCondition
				});
				// 判定範囲後のノーツを巻き込むので「before」(該当ノーツの手前判定を参照する)
				var graceS = graceFrameFinishedSkill.before;
				
				// 2行目の<=は、終点時なら確実に許容可能なので<から修正
				var secOfAfterSkill = this$1.convertFrameToSecond(graceS, obj);
				var skillEnd = shiftedSkillNoteTiming + activeSkillDuration;
				var skillEndFix = this$1.deleteMinFraction(skillEnd);
				var skillAfterNoteMargin = shiftedSkillNoteTiming + activeSkillDuration + secOfAfterSkill;
				var skillAfterNoteFixed = this$1.deleteMinFraction(skillAfterNoteMargin);
				var notesTiming = this$1.deleteMinFraction(e.timing);
				var isContain = notesTiming > skillEndFix 
					&& notesTiming <= skillAfterNoteFixed;
				
				return isContain;

			}).map(function (e) {
				// 終了時タイミングに叩く
				e.defaultTiming = e.timing;
				e.timing = shiftedSkillNoteTiming + activeSkillDuration;
				// 判定強化時は判定をPERFECTに上書きする
				if(activeSkillData.judgeSkill){
					e.judgeOverride = "perfect";
				}
				return e;
			});
			// 処理開始/終了地点はすでにずらしtiming処理を終わらせているので 
			// sc_start, sc_endと同じタイミング指定でいい
			var sc_start = shiftedSkillNoteTiming;
			var sc_end = (sc_start + activeSkillDuration);
			var notesDetail = this$1.generateNotesDetail(chartObj,
				sc_start, sc_end, sc_start, sc_end,
				activeSkillUpRate,
				obj);
			
			// 各ノーツのズレ度合い(JUSTタイミングからの変化の度合い)を算出
			// 単純な和
			var shiftFrameSum = notesDetail.reduce(function (a,x) {
				var diff = x.defaultTiming ? x.defaultTiming - x.timing : 0;
				var diffFrame = this$1.convertSecondToFrame(diff, obj);
				return a + diffFrame;
			}, 0);
			// 2乗値(距離)
			var shiftCoefficient = notesDetail.reduce(function (a,x) {
				var diff = x.defaultTiming ? x.defaultTiming - x.timing : 0;
				var diffFrame = this$1.convertSecondToFrame(diff, obj);
				return a + Math.pow(diffFrame, 2);
			}, 0);
			
			return {
				notesDetail: notesDetail,
				notesOfSkillArea: notesOfSkillArea,
				notesOfSkillBefore: notesOfSkillBefore,
				notesOfSkillAfter: notesOfSkillAfter,
				defaultSkillNoteTiming: targetSkillNote.defaultTiming,
				shiftedSkillNoteTiming: shiftedSkillNoteTiming,
				shiftFrameSum: shiftFrameSum,
				shiftCoefficient: shiftCoefficient
			}
		});
		
		return allPatternOfSkillNoteFrame;
	},
	
	// timing幅と判定を投げることでノーツ情報を生成する
	generateNotesDetail: function (chartObjects, start, end, sc_start, sc_end, sc_rate, obj) {
		var this$1 = this;

		// ソート前に元々の位置を付与
		chartObjects = chartObjects.map(function (e, i) {
			e.index = i;
			return e;
		});
		// timingでソートしておく
		chartObjects = this.sortChartData(chartObjects);
		return chartObjects.map(function (e,i) {
			// combo用index情報を付与しておく(filterを通すので予め)
			e.comboIndex = i + 1;
			return e;
		}).filter(function (e) {
			// timingでフィルターをかける
			return e.timing >= start && e.timing <= end;
		}).map(function (e) {
			// 判定幅
			var oneFS = this$1.convertFrameToSecond(1, obj);
			var sameLineSkillActive = obj.setting.sameLineSkillActive;
			// 判定指定
			var judge = "";
			var diffTiming = e.defaultTiming > 0 ? (e.timing - e.defaultTiming) : 0;
			var diffFrame = this$1.convertSecondToFrame(diffTiming, obj);
			var checkGreatJudgeTiming = function () {
				if(!e.defaultTiming){ return false; }
				
				var usingFrames = this$1.calcLongNoteJudgeFrames(e, obj, {
					isPerfect: true,
				});
				var usingFrame = diffTiming >= 0 ? usingFrames.after : usingFrames.before;
				var uf2Sec = this$1.convertFrameToSecond(usingFrame, obj);
				var diffFixed = this$1.deleteMinFraction(Math.abs(diffTiming));
				var usFixed = this$1.deleteMinFraction(uf2Sec);
				
				// USE SEC.
				return (e.defaultTiming > 0 && diffFixed > usFixed);
				// USE FRAME
				// return (e.defaultTiming > 0 && Math.abs(diffFrame) > usingFrame);
			};
			
			if(e.judgeOverride){
				// 判定強化時はその判定にする
				judge = e.judgeOverride;
			} else if(checkGreatJudgeTiming()){
				// パフェ判定を外れてたらGreat
				judge = "great";
			} else {
				judge = "perfect";
			}
			
			// TODO: 同時押しでもスキルがのるかチェック
			var isNotSkillTiming = (e.effect === "Skill") || (!sameLineSkillActive && e.timing === sc_start);
			return {
				judge: judge,
				cmb: e.comboIndex,
				index: e.index,
				effect: e.effect,
				property: e.property,
				timing: e.timing,
				defaultTiming: e.defaultTiming,
				lane: e.lane,
				beat: e.beat,
				diffFrame: diffFrame,
				scoreUpRate: (!isNotSkillTiming ? sc_rate : 0),
				scoreUpStart: (!isNotSkillTiming ? sc_start : undefined),
				scoreUpEnd: (!isNotSkillTiming ? sc_end : undefined),
			}
		});
		
		
	},
	
	// ノーツ情報から各ノーツにおけるスコアを計算する
	getScoreOfNotes: function(stateOfNotes, obj){
		var this$1 = this;

		var bandTotalValue = obj.bandTotalValue;
		var chartDifficult = obj.songLevel;
		var chartTotalNotes = obj.songChartData.metadata.combo;
		
		// sort
		stateOfNotes = stateOfNotes.sort(function (a,b) {
			return a.timing - b.timing;
		}).map(function (e, i) {
			// コンボ数をここで振り直す
			e.cmb = i + 1;
			return e;
		});
		
		return stateOfNotes.map(function (e, i) {
			// 関数が渡されてたら実行
			var scur = e.scoreUpRate, sr = 0;
			if(!obj.setting.ignoreSkill) {
				if(typeof scur !== "function") {
					sr = scur;
				}else {
					sr = scur(e, i, stateOfNotes);
				}
			}
			// 倍率代入
			e.judgeRate = this$1.calcRatesOfJudge(e.judge);
			e.comboRate = this$1.calcRatesOfCombo(e.cmb);
			e.skillRate = sr;
			e.calcScore = this$1.calcScoreOfSingleNote(
				bandTotalValue, chartDifficult, chartTotalNotes,
				e.judge, e.cmb, sr
			);
			return e.calcScore;
		}); 
	},
	
	// 1ノーツあたりのスコアを計算する基礎関数
	calcScoreOfSingleNote: function(bandTotalValue, songDifficult, totalNotes, timingJudge, comboNum, skillRate){
		return Math.floor(
			Math.floor(
				bandTotalValue * 3 * ((songDifficult - 5) * 0.01 + 1)
				/ totalNotes * this.calcRatesOfJudge(timingJudge) * this.calcRatesOfCombo(comboNum)
			) * (1 + skillRate / 100)
		)
	},
	
	// 譜面データをtiming順にSortingする/同時押しはSkillNote優先
	sortChartData: function(chartObjects){
		return chartObjects
			.filter(function (e) { return e.type !== "System"; })
			.sort(function (a, b) {
				// 同じtimingならSkillノーツが先にくるように調整しておく
				if (a.timing > b.timing) { return +1; }
				if (a.timing < b.timing) { return -1; }
				if (a.effect === "Skill" && b.effect !== "Skill") { return -1; }
				if (a.effect !== "Skill" && b.effect === "Skill") { return +1; }
			});
	},
	
	// 2つの譜面データを合成する
	conbineChartData: function(base, add){
		return base.map(function (e,i) {
			var isAddIndex = add.map(function (e) { return e.index; }).indexOf(i);
			if(isAddIndex >= 0){
				e = add.find(function (e) { return e.index === i; });
			} else {
				// スキル補正は0に戻す
				e.scoreUpRate = 0;
			}
			return e;
		});
	},
	
	// スキルデータからスキルレート関数を作成する
	createSkillRateFromSkillData: function(obj, skillData){
		var s = skillData;
		var sr = s.scoreUpRate, sr2 = s.scoreUpRate2;
		if(s.scoreUpRateWOB > 0){
			// バンド制約がかかっている場合はここで確認する
			if(this.isNotMatchUnitExisted(obj, s)){
				sr  = s.scoreUpRateWOB;
				sr2 = (s.scoreUpRate2WOB > 0) ? s.scoreUpRate2WOB : sr2;
			}
		}
		
		if(!s.isPerfectOnly && !s.isContinuedPerfectOnly){
			// 特に条件なし
			return sr;
		}
		else if(s.isContinuedPerfectOnly){
			// パフェ継続ならuprate, GREATがあったらuprate2
			return function (e, i, notes) {
				var isExistGreat = notes.some(function (e_,i_) {
					return (e_.timing >= e.scoreUpStart) && (e_.timing <= e.scoreUpEnd)
						&& (i_ <= i) && (e_.judge !== "perfect");
				});
				return !isExistGreat ? sr : sr2;
			};
		}
		else if(s.isPerfectOnly){
			// パフェならuprate, それ以外なら0
			return function (e, i, notes) {
				return (e.judge === "perfect") ? sr : 0;
			};
		}
	},
	
	// スキル発動時のユニット条件を確認する
	isNotMatchUnitExisted: function(obj, skillData){
		var cards = obj.setCards;
		// バンド条件
		var sBand = skillData.conditionBandId;
		var isNotMatchedBand = sBand && !!cards.find(function (e) {
			return bacName.charaId2BandId(e.characterId) !== sBand;
		});
		
		// タイプ条件
		var sAttr = skillData.conditionAttr;
		var isNotMatchedAttr = sAttr && !!cards.find(function (e) { return e.attr !== sAttr; });
		
		return isNotMatchedBand || isNotMatchedAttr;
	},
	
	// LNの設定に応じて判定幅を計算しなおす
	calcLongNoteJudgeFrames: function (note, obj, options) {
		if ( options === void 0 ) options = {};

		var isPerfect = options.isPerfect;
		var judgeSkillTarget = options.judgeSkillTarget;
		var st = obj.setting;
		// GREATの幅
		var gr_f = Number(!isPerfect && !judgeSkillTarget ? st.frameGreat : 0);
		// 判定強化がある場合はGOOD以下も拾う
		var ud_g = 0;
		if(judgeSkillTarget === "good") { ud_g = DefaultGoodFrames; } // GOOD: 1F
		if(judgeSkillTarget === "bad")  { ud_g = DefaultGoodFrames + DefaultBadFrames; } // GOOD+BAD: 2F
		// 判定幅として出力する値
		var def_f = Number(st.framePerfect) + gr_f + ud_g;
		var ln_f = def_f + Number(st.frameLNsweets);
		var sld_srt_f = Number(st.frameSlStart) || ln_f;
		var sld_end_f = Number(st.frameSlEnd) || DefaultPf2BadFrame;
		
		// 判定幅(手前/奥)
		var before = 0, after = 0;
		
		if(this.checkNormalFlickNote(note)) {
			// フリックの場合は押してから判定まで差がある
			var minf = st.flickMinF;
			var maxf = st.enableFlickLateGraced ? st.flickMaxF : 0;
			// 手前判定を狭くする
			before = def_f - flickMinF;
			// 奥判定をその分広くする
			after = def_f + maxf;
			
		} else if(this.checkLNEndFlickNote(note)) {
			// LN終わりのスライドの場合は普通と同じ(？)
			before = after = def_f;
			
		} else if(this.checkSlideTickNote(note)) {
			// スライドの中点は手前にはずらせない
			before = 0;
			// 後ろ側にはずらせる(標準ではOFF)
			var after_d = st.enableSlideTickGraced ? sld_end_f : 0;
			// 次のスライドノーツになると無効化される
			var after_n = this.calcDiffFrameToNextSlideNote(note, obj);
			// 両者のうち小さい方を採用
			after = Math.min(after_d, after_n);
			
		} else if(this.checkSlideEndNote(note)) {
			// Slide終点
			before = ln_f;
			after = sld_end_f;
			
		} else if(this.checkSlideStartNote(note)) {
			// Slide始点
			before = ln_f;
			// 次のスライドノーツになると無効化される
			var after_n$1 = this.calcDiffFrameToNextSlideNote(note, obj);
			// 通常の判定と比較して小さい方を採用
			after = Math.min(sld_srt_f, after_n$1);
			
		} else if(this.checkLongNote(note)) {
			// LNは判定が1F甘い(らしい)
			before = after = ln_f;
			
		} else {
			before = after = def_f;
		}
		
		before = Math.max(before, 0);
		after = Math.max(after, 0);
		// 判定幅objectを返す
		return {
			before: before, after: after
		}
	},
	
	// 次のSlideNotesを探索して現在ノーツとのF差を返す関数
	calcDiffFrameToNextSlideNote: function (note, obj) {
		var this$1 = this;

		var charts = obj.songChartData.objects;
		// TODO: TOO HEAVY FUNCTION ---------------
		var noteDefaultTiming = note.defaultTiming || note.timing;
		var baseNoteSide = this.checkSlideAB(note);
		var nextSlide = charts.find(function (e) {
			var et = e.timing;
			var side = this$1.checkSlideAB(e);
			return et > noteDefaultTiming
				&& baseNoteSide === side
				&& (this$1.checkSlideTickNote(e) || this$1.checkSlideEndNote(e))
		});
		if(!nextSlide){
			// TODO: ERROR(とりあえず無視)
			return 114514;
		}
		var nextSlideTiming = nextSlide.timing;
		var n_diff = nextSlideTiming - noteDefaultTiming - this.convertFrameToSecond(0.5, obj);
		return this.convertSecondToFrame(n_diff, obj);
	},
	
	// スライドのAB判定用関数
	checkSlideAB: function (note) {
		var e = note.effect;
		if(!e.includes("Slide"))    { return null; }
		if(e.includes("_A"))        { return "A"; }
		if(e.includes("_B"))        { return "B"; }
		return null;
	},
	
	// LN開始/終点ノーツかどうかを判定
	checkLongNote: function (note) {
		var p = note.property, e = note.effect;
		return p.includes("LongStart")  || p.includes("LongEnd")
			|| e.includes("SlideStart") || e.includes("SlideEnd")
		// --- SLOW
		// return note.property.match(/Long(Start|End)/) || note.effect.match(/Slide(Start|End)/);
	},
	
	// LN終点ノーツかどうかを判定
	checkLongNoteEnd: function (note) {
		return note.property.includes("LongEnd")
			|| note.effect.includes("SlideEnd");
	},
	
	// スライド終点ノーツかどうかを判定
	checkSlideStartNote: function (note) {
		return note.effect.includes("SlideStart");
	},
	
	// スライド終点ノーツかどうかを判定
	checkSlideEndNote: function (note) {
		return note.effect.includes("SlideEnd");
	},
	
	// スライド中点ノーツかどうかを判定
	checkSlideTickNote: function (note) {
		var e = note.effect;
		return e.includes("Slide_A") || e.includes("Slide_B")
		// --- SLOW
		// return note.effect.match(/Slide_[AB]/);
	},
	
	// 普通のフリックノーツかどうかを判定
	checkNormalFlickNote: function (note) {
		return note.effect === "Flick";
	},
	
	// LN終点フリックノーツかどうかを判定
	checkLNEndFlickNote: function (note) {
		var i = function (t) { return note.effect.includes(t); };
		return i("Flick") && (i("Long") || i("Slide"));
	},
	
	// FPSとフレーム数を指定して実際の秒数に変換
	convertFrameToSecond: function(f, obj) {
		return f/obj.setting.framePerSecond;
	},
	
	// プログラム特有の小数点の端数を消す
	// 実秒数に何か付与する等の計算をして比較を行う時に通す
	deleteMinFraction: function(v, d) {
		if ( d === void 0 ) d = 6;

		var pow10 = Math.pow(10, d);
		return Math.round(v * pow10) / pow10;
	},
	
	// FPSと秒数を指定してフレーム数に変換
	convertSecondToFrame: function(s, obj) {
		// 最小単位に合わせて算出
		var f = s * obj.setting.framePerSecond;
		var ff = this.deleteMinFraction(f); // 小数点端数を捨てる
		var m = obj.setting.dot5frameEnabled ? 0.5 : 1;
		// 切り上げで処理したほうが諸々確実な気がする
		return Math.ceil(ff / m) * m;
	},
	
	// FPS的にありえない秒数にならないように切り上げ(or切り下げ)で秒数を調整する
	shiftNoteTimingWithFrame: function (s, obj, func) {
		if ( func === void 0 ) func = "round";

		if(!obj.setting.enableStrictFrames){
			return s;
		}
		var f = obj.setting.framePerSecond;
		var m = obj.setting.dot5frameEnabled ? 0.5 : 1;
		var fm = f/m;
		return Math[func](s * fm) / fm;
	},
	
	// 判定から率を算出
	calcRatesOfJudge: function (judge) {
		return {
			"perfect": 1.1, "great": 0.8, "good": 0.5, "bad": 0, "miss": 0,
		}[judge];
	},
	
	// コンボ数から率を算出
	calcRatesOfCombo: function (combo) {
		if(combo <= 20)         { return 1.00; }
		else if(combo <=  50)   { return 1.01; }
		else if(combo <= 100)   { return 1.02; }
		else if(combo <= 150)   { return 1.03; }
		else if(combo <= 200)   { return 1.04; }
		else if(combo <= 250)   { return 1.05; }
		else if(combo <= 300)   { return 1.06; }
		else if(combo <= 400)   { return 1.07; }
		else if(combo <= 500)   { return 1.08; }
		else if(combo <= 600)   { return 1.09; }
		else if(combo <= 700)   { return 1.10; }
		else                    { return 1.11; }
		
		/* --- TOO LATE CODE ---- 
		const RatesOfComboTable = {
			  "0": 1.00,  "21": 1.01,  "51": 1.02, "101": 1.03, "151": 1.04, "201": 1.05,
			"251": 1.06, "301": 1.07, "401": 1.08, "501": 1.09, "601": 1.10, "701": 1.11,
		};
		var findKey = Object.keys(RatesOfComboTable).reverse().find(e => e <= combo);
		return RatesOfComboTable[findKey];
		*/
	},
	
	// 並び順からスコア順文字列にConvert
	getPatternToString: function (e, cs) {
		return e.map(function (e) {
			var c = cs[e - 1];
			var s = c.skillData;
			var sl = Number(c.skillLevel);
			//console.log(s);
			var r = (s.scoreUpRate) + "\n\t\t\t\t" + (s.isContinuedPerfectOnly ? "G" : "") + "\n\t\t\t\t" + (s.isPerfectOnly && !s.isContinuedPerfectOnly ? "P" : "") + "\n\t\t\t\t" + (sl !== 5 ? ("(L" + sl + ")") : "");
			return r.replace(/\s/g, "");
		}).join("→");
	},
	
	// -------------------------------
	// obsを使って進捗を送信
	_dobj: {},
	debugMsg: function (obj, message) {
		this._dobj = Object.assign(this._dobj, message);
		var msgs = Object.keys(message).map(function (i) {
			var m = message[i];
			// var t = new Array(Number(i)+1).fill(">").join("");
			// console.info(`${t} ${m}`);
			var s = new Array(i-0).fill("  ").join("");
			return (s + "> " + m);
		});
		msgs.forEach(function (e) { return console.debug(e); });
		
		//obj.Observer.trigger("calculate_info", msgs, this._dobj);
	},
	
	// -------------------------------
	// 結果Objectからだいたいの内容が記載されてるtextを錬成する
	generateResultAbstract: function (obj, res) {
		var copyright = "gbp.now.sh";
		var separator = "<!------------------------------->";
		// 内訳カード
		var showCardDetail = obj.setCards.map(function (e) {
			return ("* [ " + (e.title) + " ] " + (e.characterName) + "\n\t\t\t\t* " + (e.skillSimpleDescription.replace(/\n/g, "").trim()));
		}).join("\n");
		// AJ時含有ノーツ
		var containedNotesOfAJ = res.allJustBestSkillOrder.containSkillNotes.map(function (e) {
			return ("" + e);
		}).join(", ");
		// BST時含有ノーツ
		var containedNotesOfBest = res.bestTimingList.map(function (e) {
			var be = e.bestPatternNoteDetailExample;
			var bes = e.containAllNotesCount;
			var begn = e.bestPatternNoteDetailExample.filter(function (e) {
				return e.effect !== "Skill" && e.judge !== "perfect";
			}).length;
			return ("" + bes + (begn > 0 ? ("(GR:" + begn + ")") : ""));
		}).join(", ");;
		// BST時総Gr数
		var containedGreatNotes = (function (bea) {
			var grs = bea.filter(function (e) {
				return e.judge === "great";
			});
			var gr_pos = grs.map(function (e) {
				return e.index + 1;
			}).join(",");
			return ("" + (grs.length) + (grs.length > 0 ? (" [@" + gr_pos + "]") : ""));
		})(res.bestTimingNoteDetailExamples);
		// 許容F数
		var graceFrames = res.bestTimingList.map(function (e) {
			var countdup = obj.setting.dot5frameEnabled ? 2 : 1;
			return ((e.bestPatternsCount / countdup) + "F");
		}).join(", ");
		// スキルノーツのずらし幅
		var shiftedNotes = res.bestTimingNoteDetailExamples.filter(function (e) {
			return e.diffFrame !== 0;
		});
		var shiftedNotesToString = shiftedNotes.map(function (e) {
			var df = numeral(e.diffFrame).format("+0");
			var is_s = e.effect === "Skill";
			return (df + "F[" + (is_s ? "*" : "") + (e.index + 1) + "]");
		}).join(", ");
		
		var genAbst = "\n\t\t\t# バンドリ スコア計算ツール 結果簡易出力\n\t\t\t" + separator + "\n\t\t\t## 曲データ\n\t\t\t曲名\n\t\t\t: " + (obj.songData.title) + " ( ID: " + (obj.songData.musicId) + " )\n\t\t\t\n\t\t\t難易度\n\t\t\t: " + (obj.songDifficult) + " ( Lv." + (obj.songLevel) + " / TotalNotes: " + (obj.songChartData.metadata.combo) + " )\n\t\t\t\n\t\t\t" + separator + "\n\t\t\t## カード編成\n\t\t\t" + showCardDetail + "\n\t\t\t\n\t\t\tスキル発動順\n\t\t\t: " + (obj.setting.skillManual || "(未指定)") + "\n\t\t\t\n\t\t\tバンド総合力\n\t\t\t: " + (obj.bandTotalValue) + "\n\t\t\t\n\t\t\t" + separator + "\n\t\t\t## 計算結果\n\t\t\t### ALL-JUST\n\t\t\t合計スコア\n\t\t\t: " + (res.allJustTotalScore) + "\n\t\t\t\n\t\t\tスキル発動順\n\t\t\t: " + (res.allJustBestSkillOrder.patternToString) + "\n\t\t\t\n\t\t\tスキル内含ノーツ数\n\t\t\t: " + containedNotesOfAJ + "\n\t\t\t\n\t\t\t### BEST\n\t\t\t合計スコア\n\t\t\t: " + (res.bestTotalScore) + "\n\t\t\t\n\t\t\tスキル発動順\n\t\t\t: " + (res.bestScorePattern.patternToString) + "\n\t\t\t\n\t\t\tスキル内含ノーツ数\n\t\t\t: " + containedNotesOfBest + "\n\t\t\t\n\t\t\t全体GREAT数\n\t\t\t: " + containedGreatNotes + "\n\t\t\t\t\n\t\t\t各スキルノーツにおける許容フレーム数\n\t\t\t: " + graceFrames + "\n\t\t\t\n\t\t\tずらし内訳\n\t\t\t: " + shiftedNotesToString + "\n\t\t\t\t\n\t\t\t" + separator + "\n\t\t\t## 計算使用オプション\n\t\t\tFPS\n\t\t\t: " + (obj.setting.framePerSecond) + "\n\t\t\t\n\t\t\t判定幅\n\t\t\t: PERFECT: " + (obj.setting.framePerfect) + " / GREAT: " + (obj.setting.frameGreat) + "\n\t\t\t: PERFECT(LN): " + (obj.setting.frameLNPerfect) + "\n\t\t\t\n\t\t\t" + separator + "\n\t\t\t## generate by\n\t\t\t" + copyright + "\n\t\t";
		// source code由来のtab文字を消す
		var replacedTabCount = genAbst.split("\n")[1].split("").filter(function (e) { return e === "\t"; }).length;
		var replaceTabRegex = new RegExp(("^\\t{" + replacedTabCount + "}"), "g");
		var abstract = genAbst.split("\n").map(function (e) { return e.replace(replaceTabRegex, ""); }).join("\n").trim();
		
		return {
			abstract: abstract,
			showCardDetail: showCardDetail,
			containedNotesOfAJ: containedNotesOfAJ,
			containedNotesOfBest: containedNotesOfBest,
			containedGreatNotes: containedGreatNotes,
			graceFrames: graceFrames,
			shiftedNotes: shiftedNotes,
			shiftedNotesToString: shiftedNotesToString
		};
	},
	
	// for Csharp class code
	generateReuseCode: function (obj, opt) {
		var this$1 = this;

		// CONSTANT
		var MAX_RANDOMIZE_TAPNUM = 0;
		var MAX_RANDOMIZE_DEC_SEC = 0.005;
		var CONV_BEAT_TO_NOTEPOS_COEFF = 48;
		// param
		var totalScore = opt.totalScore;
		var notesDetail = opt.notesDetail;
		var pattern = opt.pattern;
		
		// variable
		var chart = notesDetail.sort(function (a,b) {
			return a.timing - b.timing;
		});
		var allNotesNum = chart.length;
		var needMinTapNums = chart.filter(function (e) {
			return !this$1.checkLongNoteEnd(e) && !this$1.checkSlideTickNote(e);
		}).length;
		var tapNums = Math.floor(needMinTapNums + Math.random() * MAX_RANDOMIZE_TAPNUM);
		// スコアMAXのうち最初のノーツをpickupしているぽい
		// 最後のを反映するなら.reverse()を付け足す
		var maxScoreNote = clone(chart).sort(function (a,b) {
			return b.calcScore - a.calcScore;
		})[0];
		var maxScoreVal = maxScoreNote.calcScore;
		var maxScoreCombo = maxScoreNote.cmb;
		var msSkillFactor = numeral(1 + (maxScoreNote.skillRate/100 || 0)).format("0.000000");
		var msJudge = maxScoreNote.judge;
		var msJudgeCapitalize = msJudge.charAt(0).toUpperCase() + msJudge.slice(1);
		var musicDiff = obj.songDifficult;
		var score = totalScore;
		var combo = allNotesNum;
		var greatCount = chart.filter(function (e) { return e.judge.includes("great"); }).length;
		var perfectCount = combo - greatCount;
		var clearStatus = greatCount > 0 ? "full_combo" : "all_perfect";
		var isEventFin = (new Date() - new Date(obj.eventData.publicEndAt - 0)) > 0;
		var eventId = !isEventFin ? obj.eventData.eventId : 0;
		var userDeckTotalPower = obj.bandTotalValue;
		// skill lists
		var skillActivateInfoList = pattern.map(function (i, n) {
			// card data get
			var c = obj.setCards[i-1];
			var ssid = c.skill.cardId;
			var sid = c.skill.skillId;
			var slv = c.skillData.lv;
			var activeBase = (c.skillData.scoreDescription - 0);
			var activeTime = activeBase + this$1.convertFrameToSecond(1, obj) - Math.random() * MAX_RANDOMIZE_DEC_SEC;
			var skillNotes = chart.filter(function (e) { return e.effect.includes("Skill"); })[n];
			var skillNotesTiming = Math.round(skillNotes.beat * CONV_BEAT_TO_NOTEPOS_COEFF);
			return {
				num: n+1,
				situationSkillId: ssid,
				skillId: sid,
				skillLevel: slv,
				activeSecond: numeral(activeTime).format("0.00000"),
				notesPosition: skillNotesTiming
			}
		});
		var skillActivateInfoListStr = skillActivateInfoList.map(function (e) {
			var r = "new SkillActivateInfo {\n\t\t\t\tnum=" + (e.num) + ",\n\t\t\t\tsituationSkillId=" + (e.situationSkillId) + ",\n\t\t\t\tskillId=" + (e.skillId) + ",\n\t\t\t\tskillLevel=" + (e.skillLevel) + ",\n\t\t\t\tactiveSecond=" + (e.activeSecond) + "f,\n\t\t\t\tnotesPosition=" + (e.notesPosition) + "\n\t\t\t}";
			return r.replace(/(\n|\t)/g, " ").replace(/\s{2,}/g, " ");
		}).join(",\n\t\t\t");
		
		// generate object
		var musicResultObj = {
			musicDifficulty: musicDiff,
			score: score, combo: combo, clearStatus: clearStatus, eventId: eventId,
			perfectCount: perfectCount, greatCount: greatCount,
			goodCount: 0,
			badCount: 0,
			missCount: 0,
			userDeckTotalPower: userDeckTotalPower,
			totalNotesCount: allNotesNum,
			tapCount: tapNums,
			oneNotesMaxScoreInfo: {
				score: maxScoreVal,
				combo: maxScoreCombo,
				skillFactor: msSkillFactor,
				notesType: msJudgeCapitalize,
				isFever: false,
			},
			finalLife: 1000,
			skillActivateInfoList: {
				entries: skillActivateInfoList
			}
		};
		
		// generate code of C#
		var csharpCode = "var songResult = new songResultRequest {\n\ttotalNotesCount = " + allNotesNum + ",\n\ttapCount = " + tapNums + ",\n\toneNotesMaxScoreInfo = new OneNotesMaxScoreInfo {\n\t\tscore = " + maxScoreVal + ",\n\t\tcombo = " + maxScoreCombo + ",\n\t\tskillFactor = " + msSkillFactor + "f,\n\t\tnotesType = \"" + msJudgeCapitalize + "\",\n\t\tisFever = false\n\t},\n\tfinalLife = 1000,\n\tskillActivateInfoList = new SkillActivateInfoList{\n\t\tentries = new SkillActivateInfo[] {\n\t\t\t" + skillActivateInfoListStr + "\n\t\t}\n\t},\n\tmusicDifficulty = \"" + musicDiff + "\",\n\tscore = " + score + ",\n\tcombo = " + combo + ",\n\tclearStatus = \"" + clearStatus + "\",\n\teventId = " + eventId + ",\n\tperfectCount = " + perfectCount + ",\n\tgreatCount = " + greatCount + ",\n\tgoodCount = 0,\n\tbadCount = 0,\n\tmissCount = 0,\n\tuserDeckTotalPower = " + userDeckTotalPower + "\n};";

		return {
			musicResultObj: musicResultObj,
			csharpCode: csharpCode,
		};
	}
	
};
},{"../bndrUtil/bandAndCharaName.js":4,"../util/calcArraySum.js":15,"fast-clone":150,"js-combinatorics":172,"numeral":178}],13:[function(require,module,exports){
var calcArraySum = require("../util/calcArraySum.js");
var calcArrayMode = require("../util/calcArrayMode.js");
var bandAndCharaName = require("../bndrUtil/bandAndCharaName.js");

var paramName = ["performance", "technique", "visual"];

module.exports = {
	// もらったデータから計算させる
	calcTotalValuesFromOpts: function (cards, opts) {
		var d = opts.event.detail;
		var isChallengeBonus = !opts.is_free && ["challenge", "versus"].includes(opts.event.eventType);
		var options = {
			characters: d.characters,
			attributes: d.attributes,
			paramBonus: d.paramBonus,
			isChallengeBonus: isChallengeBonus
		};
		// バンド特化
		var bandShape = this.calcTotalValues(cards, options, false);
		// 共用使用
		var sharedCheck = this.calcTotalValues(cards, options, true);
		return Math.max(bandShape, sharedCheck);
	},
	
	// 計算本体
	// WARNING: レアリティ3/4にしか対応してないので注意
	calcTotalValues: function (cards, options, sharedUse) {
		var totalVal = 0;
		var characters = options.characters;
		var attributes = options.attributes;
		var paramBonus = options.paramBonus;
		var isChallengeBonus = options.isChallengeBonus;
		// 置物倍率
		var BandtypeRate;
		var AttributeRate = options.rateOfAttr || (0.12 * 2); // 12% + 12%
		var ParamRate     = options.rateOfParm || 0.16; // 16%
		if(sharedUse){
			BandtypeRate  = options.rateOfBand || (0.0225*5 + 0.055*2); // (2.25%*5)+(5.5%*2)
		} else {
			BandtypeRate  = options.rateOfBand || (0.045*5 + 0.11*2); // (4.5%*5)+(11%*2)
		}
		
		// 補正キャラ/補正属性取得
		var evCharaObj = characters; // opts.event.detail.
		var evCharas = evCharaObj.map(function (e) { return e.characterId; });
		var evCharaRate = evCharaObj.map(function (e) { return e.percent; });
		var evAttrObj = attributes; // opts.event.detail.
		var evAttr = evAttrObj.map(function (e) { return e.attribute; });
		var evAttrRate = evAttrObj.map(function (e) { return e.percent; });
		// 最も効果のある置物種類算出
		// 単純に、一番多いバンド/属性に合わせる
		var modeBand = sharedUse ? -1 : calcArrayMode(cards.map(function (e) {
			return bandAndCharaName.charaId2BandId(e.characterId);
		}));
		var modeAttr = calcArrayMode(cards.map(function (e) { return e.attr; }));
		// マガジン系は単純に一番高いパラメータに合わせる
		var paramMax = paramName.map(function (pn) {
			var sum = calcArraySum(cards.map(function (c) {
				return c.simpleParams.max[pn];
			}));
			return { pn: pn, sum: sum }
		}).sort(function (a,b) {
			return b.sum - a.sum;
		})[0];
		
		// 各カードの総合力計算
		var UpParamOfRarety3 = 1000, UpParamOfRarety4 = 1250;
		var paramOfCards = cards.map(function (e) {
			var prms = paramName.map(function (pn) {
				// 特訓/エピソード解放ボーナス
				// WARNING: めんどいのでとりあえずレアリティで決め打ち
				var upParamOfTrrAndEpsode = (e.rarity === 4) ? UpParamOfRarety4 : UpParamOfRarety3;
				var v = (e.simpleParams.max[pn] + upParamOfTrrAndEpsode);
				return {pn: pn, v: v};
			});
			e.simpleParams.prms = prms;
			return prms;
		});
		var totalValOfCards = cards.map(function (e, i) {
			var t = calcArraySum(paramName.map(function (pn) {
				var pr = e.simpleParams.prms.find(function (p) { return p.pn === pn; });
				return pr.v;
			}));
			e.simpleParams.total = t;
			return t;
		});
		totalVal += calcArraySum(totalValOfCards);
		// 
		// console.log("バンドパラメータ合計", calcArraySum(totalValOfCards), "内訳", totalValOfCards);
		
		// バンド総合力計算(置物補正をここにのせる)
		var areaItemBonus = cards.map(function (c) {
			var ab = 0;
			var total = c.simpleParams.total;
			// 全パラメータ対象
			var charaBandId = bandAndCharaName.charaId2BandId(c.characterId);
			var isMatchBand = (modeBand === -1 || charaBandId === modeBand);
			var upRateBand = isMatchBand ? BandtypeRate : 0;
			var upRateAttr = c.attr === modeAttr ? AttributeRate : 0;
			var upRate = (upRateBand + upRateAttr);
			ab += (total * upRate);
			// マガジン系
			var mgzParam = paramMax.pn;
			var prm = c.simpleParams.prms.find(function (pr) { return pr.pn === mgzParam; }).v;
			ab += (prm * ParamRate);
			return ab;
		});
		var areaItemBonusSum = calcArraySum(areaItemBonus);
		totalVal += areaItemBonusSum;
		// 
		// console.log("エリアアイテムボーナス", areaItemBonusSum, "内訳", areaItemBonus);
		
		// 編成ボーナス(両一致パラメーターボーナス)
		var totalValOfCardsOnMatchEventParamBonus = cards.map(function (e) {
			return calcArraySum(paramName.map(function (pn) {
				// WARNING: ここもレアリティ決め打ち
				var upParamOfTrrAndEpsode = (e.rarity ===  4) ? UpParamOfRarety4 : UpParamOfRarety3;
				var paramTotal = (e.simpleParams.max[pn] + upParamOfTrrAndEpsode);
				var isMatchEvCharaAndAttr = (evCharas.indexOf(e.characterId) >= 0 && evAttr.indexOf(e.attr) >= 0);
				var upParamLate = paramBonus && paramBonus.length > 0 ? paramBonus[0][pn] / 100 : 0;
				return paramTotal * (isMatchEvCharaAndAttr ? upParamLate : 0);
			}));
		});
		var totalValOnMatchEventBonus = isChallengeBonus ? calcArraySum(totalValOfCardsOnMatchEventParamBonus) : 0;
		// 
		//console.log("編成ボーナス(両一致ボーナス)", totalValOnMatchEventBonus, "内訳", totalValOfCardsOnMatchEventBonus);
		
		// 編成ボーナス(チャレンジ補正)
		if(isChallengeBonus){
			// TODO: 一時的な対応、後でevent定義の値を参照するように修正
			var CharaAndAttrMatchTotalBonus = 20;
			var challengeTotalValOfCards = totalValOfCards.map(function (e,i) {
				var c = cards[i];
				var evCharaIndex = evCharas.indexOf(c.characterId);
				var upForChara = evCharaIndex >= 0 ? evCharaRate[evCharaIndex] : 0;
				var evAttrIndex = evAttr.indexOf(c.attr);
				var upForAttr = evAttrIndex >= 0 ? evAttrRate[evAttrIndex] : 0;
				var upForCharaAndAttr = (evCharaIndex >= 0 && evAttrIndex >= 0) ? CharaAndAttrMatchTotalBonus : 0;
				return (e * (upForAttr + upForChara + upForCharaAndAttr) / 100) || 0;
			});
			var challengeTotalVal = calcArraySum(challengeTotalValOfCards);
			//
			//console.log("編成ボーナス(チャレンジ)", challengeTotalVal, "内訳", challengeTotalValOfCards);
		}
		// 算出
		totalVal += (totalValOnMatchEventBonus + (challengeTotalVal || 0));
		// 最後に切り捨て
		totalVal = Math.floor(totalVal);
		
		//console.log("編成ボーナス(合計)", totalValOnMatchEventBonus + challengeTotalVal);
		//console.log("全合計", totalVal);
		return totalVal;
	},
	
	// 手でデータを打ち込む用(カードデータは用意済)
	calcTotalValuesForSelf: function (cards, charaArray, targetAttr, paramBonusName, isChallenge,
	                                  charaPercent, attrPercent, pbPercent) {
		if ( charaPercent === void 0 ) charaPercent = 10;
		if ( attrPercent === void 0 ) attrPercent = 20;
		if ( pbPercent === void 0 ) pbPercent = 50;

		var att_ar = [targetAttr];
		var chs = charaArray.map(function (e) {
			return {
				characterId: e,
				percent: charaPercent,
			}
		});
		var ats = att_ar.map(function (e) {
			return {
				attribute: e,
				percent: attrPercent,
			}
		});
		var pbs = [{}];
		paramName.forEach(function (e) {
			pbs[0][e] = e.toLowerCase() === paramBonusName.toLowerCase() ? pbPercent : 0;
		});
		
		return this.calcTotalValues(cards, chs, ats, pbs, isChallenge);
	},
	
};
},{"../bndrUtil/bandAndCharaName.js":4,"../util/calcArrayMode.js":14,"../util/calcArraySum.js":15}],14:[function(require,module,exports){
// calculate array's mode
// ref: https://qiita.com/yas-nyan/items/8afaf7a1fc5b2306354b
module.exports = function (arr) {
	if (arr.length === 0){
		//配列の個数が0だとエラーを返す。
		throw new Error("配列の長さが0のため最頻値が計算できません");
		//nullを返しても困らない時(配列の中にnullが無い時)はnullを返すように実装しても良い。
		//return null
	}
	//回数を記録する連想配列
	var counter = {};
	//本来の値を入れた辞書
	var nativeValues = {};
	
	//最頻値とその出現回数を挿入する変数
	var maxCounter = 0;
	var maxValue = null;
	
	for (var i = 0; i < arr.length; i++) {
		//counterに存在しなければ作る。keyは型を区別する
		if (!counter[arr[i] + "_" + typeof arr[i]]) {
			counter[arr[i] + "_" + typeof arr[i]] = 0;
		}
		counter[arr[i] + "_" + typeof arr[i]]++;
		nativeValues[arr[i] + "_" + typeof arr[i]] = arr[i];
		
	}
	for (var j = 0; j < Object.keys(counter).length; j++) {
		key = Object.keys(counter)[j];
		if (counter[key] > maxCounter) {
			maxCounter = counter[key];
			maxValue = nativeValues[key]
		}
	}
	return maxValue;
};
},{}],15:[function(require,module,exports){
// calculate array's sum
module.exports = function (arr) {
	return arr.reduce(function (a,x) { return a+=x; },0);
};
},{}],16:[function(require,module,exports){
var UsageFileSize = 1024 * 1024 * 1024;

module.exports = {
	"load": async function (filename) {
		var _t = this;
		return await new Promise(async function (res, rej) {
			try {
				var fe = await _t.getFile(filename);
			} catch(e) {
				console.error(e);
				return rej(e);
			}
			var file = await new Promise(function (res) { return fe.file(res); });
			var reader = new FileReader();
			reader.onloadend = function(e){
				var r = this.result;
				var o = {};
				try {
					o = JSON.parse(r);
				} catch(e) {
				}
				res(o);
			};
			reader.onerror = rej;
			var rst = reader.readAsText(file);
		});
	},
	"save": async function (filename, obj) {
		var _t = this;
		return await new Promise(async function (res, rej) {
			try {
				var fe = await _t.getFile(filename);
			} catch(e) {
				console.error(e);
				return rej(e);
			}
			var j = JSON.stringify(obj);
			var writer = await new Promise(function (res) { return fe.createWriter(res); });
			writer.onwriteend = res;
			writer.onerror = rej;
			var blb = new Blob([j]);
			writer.write(blb);
		});
		
	},
	"getFile": async function(filename, size) {
		if ( size === void 0 ) size = UsageFileSize;

		try {
			var fs = await this.requestFileSystem(size);
			return await new Promise(function (res, rej) { return fs.root.getFile(filename, { create: true }, res, rej); });
		} catch(e) {
			console.error(e);
			throw e;
		}
	},
	"requestFileSystem": async function(size, savetype){
		if ( savetype === void 0 ) savetype = window.PERSISTENT;

		return await new Promise(function (res, rej) {
			if(size && navigator.webkitPersistentStorage){
				navigator.webkitPersistentStorage.requestQuota(size, function (granted) {
					window.webkitRequestFileSystem(savetype, granted, function (fs) {
						res(fs);
					});
				});
			} else {
				rej("navigator.webkitPersistentStorage is undefined");
			}
		});
	}
	
};
},{}],17:[function(require,module,exports){
(function (global){
var s = localStorage;
module.exports = {
	save: function(path, value) {
		if(global.storageReadOnly){
			return false;
		}
		var ps = pathSplit(path);
		var f = ps.shift(1);
		var o = JSON.parse(s.getItem(f));
		if(!o){
			o = {};
		}
		ps.reduce(function (a,x,i,arr) {
			//console.log(a,x,i);
			if(arr.length === i + 1){
				return a[x] = value;
			} else {
				return !a[x] ? a[x] = {} : a[x];
			}
		}, o);
		//console.log(f,o,ps);
		s.setItem(f, JSON.stringify(o));
		return true;
	},
	saveArrays: function(path, value, toggle) {
		var v = this.load(path);
		if(!v || !v.length){
			v = [];
		}
		var isContain = v.includes(value);
		if(!isContain && toggle){
			// add
			v.push(value);
		}
		if(isContain && !toggle){
			// remove
			v = v.filter(function (e) { return e !== value; });
		}
		// save
		return this.save(path, v);
	},
	load: function(path) {
		var val = null;
		var ps = pathSplit(path);
		var f = ps.shift(1);
		var o = JSON.parse(s.getItem(f));
		if(!o){
			return null;
		}
		ps.reduce(function (a,x,i,arr) {
			//console.log(a,x,i);
			if(arr.length === i + 1){
				return val = a[x];
			} else {
				return a[x];
			}
		}, o);
		return val;
	},
};

function pathSplit(p) {
	return p.split(".");
}
}).call(this,typeof global !== "undefined" ? global : typeof self !== "undefined" ? self : typeof window !== "undefined" ? window : {})
},{}],18:[function(require,module,exports){
// template literal like C# format
// ref: https://oar.st40.xyz/article/341
module.exports = function (str) {
	var args = [], len = arguments.length - 1;
	while ( len-- > 0 ) args[ len ] = arguments[ len + 1 ];

	return str.replace(/\{(\d+)\}/g, function (m, k) {
		return args[k];
	});
};
},{}],19:[function(require,module,exports){
module.exports = function (urls) {
	var rst = { params: [] };
	urls = Array.from(urls);
	urls.slice(1, urls.length).forEach(function (e) {
		var rg = /([a-zA-Z]*:|)(.+)/;
		var rm = e.match(rg);
		if(!rm){
			return 0;
		} else if(rm.length === 3 && rm[1].length > 0){
			var k = rm[1].replace(/:/g, "");
			rst[k] = rm[2];
		} else if(rm.length > 0){
			rst["params"].push(rm[2]);
		}
	});
	return rst;
};
